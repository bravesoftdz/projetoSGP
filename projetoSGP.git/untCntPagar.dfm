      '    DINHEIRO,'
      '    ENVIADO_WEB,'
      '    DT_ALT,'
      '    DESCRICAO,'
      '    IDPLANO_CONTA'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :POSICAO,'
      '    :PREFIXO,'
      '    :REFERENCIA,'
