      '    :IDPLANO_CONTA'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    P.*,'
      '    F.NOME FORNECEDOR,'
      '    FP.NOME FORMA_PAG'
      'FROM'
      '    CNT_PAGAR P, FORNECEDOR F, FORMA_PAGAMENTO FP'
      'WHERE( '
      '    F.ID = P.IDFORNECEDOR'
      'AND'
