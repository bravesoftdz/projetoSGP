  object fdsCntReceber: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CNT_RECEBER'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
      '    IDPORTADOR = :IDPORTADOR,'
      '    HISTORICO = :HISTORICO,'
      '    IDFORMA_PAG = :IDFORMA_PAG,'
      '    IDVENDA = :IDVENDA,'
      '    TIPO_VENDA = :TIPO_VENDA,'
      '    TOT_PARCELA = :TOT_PARCELA,'
      '    PARCELAS_PG = :PARCELAS_PG,'
      '    DINHEIRO = :DINHEIRO,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CNT_RECEBER'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO CNT_RECEBER('
      '    ID,'
      '    IDFILIAL,'
      '    DT_INC,'
      '    DT_ALT,'
      '    POSICAO,'
      '    PREFIXO,'
      '    REFERENCIA,'
      '    IDCLIENTE,'
      '    ACRESCIMO,'
      '    DESCONTO,'
      '    VALOR,'
      '    VALOR_PAGO,'
      '    IDPORTADOR,'
      '    HISTORICO,'
      '    IDFORMA_PAG,'
      '    IDVENDA,'
      '    TIPO_VENDA,'
      '    TOT_PARCELA,'
      '    PARCELAS_PG,'
      '    DINHEIRO,'
      '    ENVIADO_WEB'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :POSICAO,'
      '    :PREFIXO,'
      '    :REFERENCIA,'
      '    :IDCLIENTE,'
      '    :ACRESCIMO,'
      '    :DESCONTO,'
      '    :VALOR,'
      '    :VALOR_PAGO,'
      '    :IDPORTADOR,'
      '    :HISTORICO,'
      '    :IDFORMA_PAG,'
      '    :IDVENDA,'
      '    :TIPO_VENDA,'
      '    :TOT_PARCELA,'
      '    :PARCELAS_PG,'
      '    :DINHEIRO,'
      '    :ENVIADO_WEB'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    CNT_RECEBER '
      ''
      ' WHERE '
      '        CNT_RECEBER.ID = :OLD_ID'
      '    and CNT_RECEBER.IDFILIAL = :OLD_IDFILIAL'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    CNT_RECEBER'
      'WHERE'
      '    ID = :ID')
    Transaction = transCntReceber
    Database = DM.conexao
    Left = 504
    Top = 16
