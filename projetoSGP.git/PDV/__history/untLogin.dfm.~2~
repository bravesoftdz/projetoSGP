  object fdsUser: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE USUARIO'
      'SET '
      '    NOME = :NOME,'
      '    LOGIN = :LOGIN,'
      '    SENHA = :SENHA'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    USUARIO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO USUARIO('
      '    ID,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA'
      ')'
      'VALUES('
      '    :ID,'
      '    :NOME,'
      '    :LOGIN,'
      '    :SENHA'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA'
      'FROM'
      '    USUARIO'
      'WHERE( '
      '    LOGIN = :LOGIN'
      'AND'
      '    SENHA = :SENHA'
      '     ) and (     USUARIO.ID = :OLD_ID'
      '     )'
      '     ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA'
      'FROM'
      '    USUARIO'
      'WHERE'
      '    LOGIN = :LOGIN'
      'AND'
      '    SENHA = :SENHA ')
    Database = DM.conexao
    SQLScreenCursor = crSQLWait
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
