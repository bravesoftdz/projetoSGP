  object fdsBanco: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE BANCO'
      'SET '
      '    DESCRICAO = :DESCRICAO,'
      '    NUM_BANCO = :NUM_BANCO'
      'WHERE'
      '    NUM_BANCO = :OLD_NUM_BANCO'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    BANCO'
      'WHERE'
      '        NUM_BANCO = :OLD_NUM_BANCO'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO BANCO('
      '    DESCRICAO,'
      '    NUM_BANCO'
      ')'
      'VALUES('
      '    :DESCRICAO,'
      '    :NUM_BANCO'
      ')')
    RefreshSQL.Strings = (
      'SELECT DESCRICAO, NUM_BANCO FROM BANCO WHERE(  STATUS <> '#39'I'#39
      '     ) and (     BANCO.NUM_BANCO = :OLD_NUM_BANCO'
      '     )'
      '    ')
    SelectSQL.Strings = (
      
        'SELECT ID, DESCRICAO, NUM_BANCO FROM BANCO WHERE NUM_BANCO = :NU' +
        'MBANCO')
    Database = DM.conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 224
    Top = 8
    oFetchAll = True
    object fdsBancoDESCRICAO: TFIBStringField
      FieldName = 'DESCRICAO'
      Size = 250
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsBancoNUM_BANCO: TFIBStringField
      FieldName = 'NUM_BANCO'
      Size = 3
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsBancoID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
  end
