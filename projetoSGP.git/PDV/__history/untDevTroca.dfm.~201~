  object qryMovimento: TpFIBQuery
    Transaction = DM.trans
    Database = DM.conexao
    SQL.Strings = (
      'execute procedure gera_mov_item'
      '('
      '   :IDITEM, '
      '   :IDFILIAL, '
      '   :DT_INC, '
      '   :DESCRICAO, '
