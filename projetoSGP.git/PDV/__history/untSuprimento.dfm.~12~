  object fdsMovCaixa: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE MOVIMENTO_CAIXA'
      'SET '
      '    IDCAIXA = :IDCAIXA,'
      '    DT_INC = :DT_INC,'
      '    HR_INC = :HR_INC,'
      '    IDUSUARIO = :IDUSUARIO,'
      '    CONTROLE = :CONTROLE,'
      '    TIPO = :TIPO,'
      '    MOEDA = :MOEDA,'
      '    VLR_ENTRADA = :VLR_ENTRADA,'
      '    VLR_CHEQUE = :VLR_CHEQUE,'
      '    VLR_CARTAO = :VLR_CARTAO,'
      '    VLR_NOTA = :VLR_NOTA,'
      '    VLR_FINANCEIRA = :VLR_FINANCEIRA,'
      '    VLR_VALEDESCONTO = :VLR_VALEDESCONTO,'
      '    VLR_TICKET = :VLR_TICKET,'
      '    VLR_VENDA = :VLR_VENDA,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    VLR_DESCONTO = :VLR_DESCONTO,'
      '    TROCO = :TROCO,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    IDMOVIMENTO = :IDMOVIMENTO,'
      '    HISTORICO = :HISTORICO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    MOVIMENTO_CAIXA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO MOVIMENTO_CAIXA('
      '    ID,'
      '    IDCAIXA,'
      '    IDFILIAL,'
      '    DT_INC,'
      '    HR_INC,'
      '    IDUSUARIO,'
      '    CONTROLE,'
      '    TIPO,'
      '    MOEDA,'
      '    VLR_ENTRADA,'
      '    VLR_CHEQUE,'
      '    VLR_CARTAO,'
      '    VLR_NOTA,'
      '    VLR_FINANCEIRA,'
      '    VLR_VALEDESCONTO,'
      '    VLR_TICKET,'
      '    VLR_VENDA,'
      '    VLR_TOTAL,'
      '    VLR_DESCONTO,'
      '    TROCO,'
      '    ENVIADO_WEB,'
      '    IDMOVIMENTO,'
      '    HISTORICO'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDCAIXA,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :HR_INC,'
      '    :IDUSUARIO,'
      '    :CONTROLE,'
      '    :TIPO,'
      '    :MOEDA,'
      '    :VLR_ENTRADA,'
      '    :VLR_CHEQUE,'
      '    :VLR_CARTAO,'
      '    :VLR_NOTA,'
      '    :VLR_FINANCEIRA,'
      '    :VLR_VALEDESCONTO,'
      '    :VLR_TICKET,'
      '    :VLR_VENDA,'
      '    :VLR_TOTAL,'
      '    :VLR_DESCONTO,'
      '    :TROCO,'
      '    :ENVIADO_WEB,'
      '    :IDMOVIMENTO,'
      '    :HISTORICO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    MC.*,'
      '    U.NOME OPERADOR'
      'FROM'
      '    MOVIMENTO_CAIXA MC, USUARIO U'
      'WHERE( '
      '    U.ID = MC.IDUSUARIO'
      'AND'
      '    MC.CONTROLE = :CONTROLE'
      'AND '
      '    MC.IDCAIXA = :IDCAIXA'
      'AND '
      '    MC.IDFILIAL = :IDFILIAL'
      '     ) and (     MC.ID = :OLD_ID'
      '    and MC.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    MC.*,'
      '    U.NOME OPERADOR'
      'FROM'
      '    MOVIMENTO_CAIXA MC, USUARIO U'
      'WHERE'
      '    U.ID = MC.IDUSUARIO'
      'AND'
      '    MC.CONTROLE = :CONTROLE'
      'AND '
      '    MC.IDCAIXA = :IDCAIXA'
      'AND '
      '    MC.IDFILIAL = :IDFILIAL'
      'ORDER BY '
      '    MC.ID')
    AllowedUpdateKinds = [ukInsert, ukDelete]
    Transaction = transSuprimento
    Database = DM.conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
