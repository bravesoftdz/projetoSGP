      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    MC.*,'
      '    U.NOME OPERADOR'
      'FROM'
      '    MOVIMENTO_CAIXA MC, USUARIO U'
      'WHERE( '
      '    U.ID = MC.IDUSUARIO'
      'AND'
      '    MC.CONTROLE = :CONTROLE'
      'AND '
      '    MC.IDCAIXA = :IDCAIXA'
      'AND '
      '    MC.IDFILIAL = :IDFILIAL'
      '     ) and (     MC.ID = :OLD_ID'
      '    and MC.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
