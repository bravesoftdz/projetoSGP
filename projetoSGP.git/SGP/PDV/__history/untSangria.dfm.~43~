      '    TROCO = :TROCO,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    IDMOVIMENTO = :IDMOVIMENTO,'
      '    HISTORICO = :HISTORICO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    MOVIMENTO_CAIXA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO MOVIMENTO_CAIXA('
