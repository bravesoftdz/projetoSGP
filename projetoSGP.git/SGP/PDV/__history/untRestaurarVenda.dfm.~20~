  object fdsVenda: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '    V.*,'
      '    C.NUM_CAIXA'
      'FROM'
      '    VENDA V, CAIXA C'
      'WHERE'
      '    (V.STATUS = '#39'AB'#39' OR V.STATUS = '#39'CA'#39')'
      'AND'
      '    C.ID = V.IDCAIXA '
      'ORDER BY'
      '    V.ID DESC')
    AutoUpdateOptions.UpdateTableName = 'PREVENDA'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_PREVENDA'
    AutoUpdateOptions.WhenGetGenID = wgOnNewRecord
    AfterScroll = fdsVendaAfterScroll
    Transaction = DM.trans
    Database = DM.conexao
