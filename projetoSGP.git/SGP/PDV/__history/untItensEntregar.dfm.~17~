  object fdsVendaItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA_ITEM'
      'SET '
      '    IDPROD_ITEM = :IDPROD_ITEM,'
      '    DT_INC = :DT_INC,'
      '    QTN = :QTN,'
      '    VLR_UNITARIO = :VLR_UNITARIO,'
      '    VLR_DESCONTO = :VLR_DESCONTO,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    PESO = :PESO,'
      '    QTN_RETORNO = :QTN_RETORNO,'
      '    MOTIVO_RETORNO = :MOTIVO_RETORNO,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    ENTREGAR = :ENTREGAR'
      'WHERE'
      '    IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA_ITEM'
      'WHERE'
      '        IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA_ITEM('
      '    IDVENDA,'
      '    ITEM,'
      '    IDPROD_ITEM,'
      '    DT_INC,'
      '    QTN,'
      '    VLR_UNITARIO,'
      '    VLR_DESCONTO,'
      '    VLR_TOTAL,'
      '    PESO,'
      '    QTN_RETORNO,'
      '    MOTIVO_RETORNO,'
      '    ENVIADO_WEB,'
      '    IDFILIAL,'
      '    ENTREGAR'
      ')'
      'VALUES('
      '    :IDVENDA,'
      '    :ITEM,'
      '    :IDPROD_ITEM,'
      '    :DT_INC,'
      '    :QTN,'
      '    :VLR_UNITARIO,'
      '    :VLR_DESCONTO,'
      '    :VLR_TOTAL,'
      '    :PESO,'
      '    :QTN_RETORNO,'
      '    :MOTIVO_RETORNO,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL,'
      '    :ENTREGAR'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    VENDA_ITEM'
      'WHERE( '
      '    IDVENDA = :IDVENDA'
      'AND'
      '    IDFILIAL = :IDFILIAL'
      'AND'
      '    ITEM = :ITEM'
      '     ) and (     VENDA_ITEM.IDVENDA = :OLD_IDVENDA'
      '    and VENDA_ITEM.ITEM = :OLD_ITEM'
      '    and VENDA_ITEM.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '     ')
    SelectSQL.Strings = (
      'SELECT'
      '    VI.*,'
      '    PI.NOME'
      'FROM'
      '    VENDA_ITEM VI, PRODUTO_ITEM PI'
      'WHERE'
      '    IDVENDA = :IDVENDA'
      'AND'
      '    IDFILIAL = :IDFILIAL'
      'AND'
      '    ITEM = :ITEM '
      'AND'
      '    PI.ID = VI.IDPROD_ITEM'
      'ORDER BY ITEM'
      '')
    Transaction = DM.trans
    Database = DM.conexao
    SQLScreenCursor = crSQLWait
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 168
    Top = 8
    oFetchAll = True
    object fdsVendaItensIDVENDA: TFIBBCDField
