  object fdsServicos: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE SERVICO'
      'SET '
      '    STATUS = :STATUS,'
      '    DT_INC = :DT_INC,'
      '    NOME = :NOME,'
      '    DESCRICAO = :DESCRICAO,'
      '    PRECO = :PRECO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    SERVICO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO SERVICO('
      '    ID,'
      '    STATUS,'
      '    DT_INC,'
      '    NOME,'
      '    DESCRICAO,'
      '    PRECO'
      ')'
      'VALUES('
      '    :ID,'
      '    :STATUS,'
      '    :DT_INC,'
      '    :NOME,'
      '    :DESCRICAO,'
      '    :PRECO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    SERVICO'
      ''
      ' WHERE '
      '        SERVICO.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    SERVICO')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.trans
    Database = DM.conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 16
    Top = 8
    oFetchAll = True
