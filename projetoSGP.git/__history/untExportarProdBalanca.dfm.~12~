  object fdsProdutoKG: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PRODUTO_ITEM'
      'SET '
      '    IDPRODUTO = :IDPRODUTO,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDTAMANHO = :IDTAMANHO,'
      '    IDCOR = :IDCOR,'
      '    PESO = :PESO,'
      '    PREC_CUSTO = :PREC_CUSTO,'
      '    COD_FABRICA = :COD_FABRICA,'
      '    PREC_ULTIMO_CUSTO = :PREC_ULTIMO_CUSTO,'
      '    DT_VALIDADE = :DT_VALIDADE,'
      '    UNIDADE = :UNIDADE,'
      '    UTILIZA_BALANCA = :UTILIZA_BALANCA'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PRODUTO_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO_ITEM('
      '    ID,'
      '    IDPRODUTO,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDTAMANHO,'
      '    IDCOR,'
      '    PESO,'
      '    PREC_CUSTO,'
      '    COD_FABRICA,'
      '    PREC_ULTIMO_CUSTO,'
      '    DT_VALIDADE,'
      '    UNIDADE,'
      '    UTILIZA_BALANCA'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDPRODUTO,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDTAMANHO,'
      '    :IDCOR,'
      '    :PESO,'
      '    :PREC_CUSTO,'
      '    :COD_FABRICA,'
      '    :PREC_ULTIMO_CUSTO,'
      '    :DT_VALIDADE,'
      '    :UNIDADE,'
      '    :UTILIZA_BALANCA'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PI.ID,'
      '    PI.IDPRODUTO,'
      '    PI.STATUS,'
      '    PI.NOME,'
      '    PI.REFERENCIA,'
      '    PI.IDTAMANHO,'
      '    PI.IDCOR,'
      '    PI.PESO,'
      '    PI.PREC_CUSTO,'
      '    PI.COD_FABRICA,'
      '    PI.PREC_ULTIMO_CUSTO,'
      '    PI.DT_VALIDADE,'
      '    PI.UNIDADE,'
      '    PI.UTILIZA_BALANCA,'
      '    PIE.PREC_VENDA'
      'FROM'
      '    PRODUTO_ITEM PI ,PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE( '
      'PI.STATUS='#39'A'#39
      'AND'
      'PI.UNIDADE ='#39'KG'#39' '
      'AND'
      'PIE.IDITEM = PI.ID'
      '     ) and (     PI.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PI.ID,'
      '    PI.IDPRODUTO,'
      '    PI.STATUS,'
      '    PI.NOME,'
      '    PI.REFERENCIA,'
      '    PI.IDTAMANHO,'
      '    PI.IDCOR,'
      '    PI.PESO,'
      '    PI.PREC_CUSTO,'
      '    PI.COD_FABRICA,'
      '    PI.PREC_ULTIMO_CUSTO,'
      '    PI.DT_VALIDADE,'
      '    PI.UNIDADE,'
      '    PI.UTILIZA_BALANCA,'
      '    PIE.PREC_VENDA'
      'FROM'
      '    PRODUTO_ITEM PI ,PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE'
      'PI.STATUS='#39'A'#39
      'AND'
      'PI.UNIDADE ='#39'KG'#39' '
      'AND'
      'PIE.IDITEM = PI.ID')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 440
    Top = 8
