  object fdsCheque: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '    CH.*,'
      '    B.DESCRICAO BANCO'
      ''
      'FROM'
      '    CNT_RECEBER_CHEQUE CH, BANCO B'
      'WHERE'
      '   CH.IDRECEBER_ITEM = :IDRECEBER_ITEM'
      'AND'
      '   B.ID = CH.IDBANCO')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 320
    Top = 8
    oFetchAll = True
    object fdsChequeID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
