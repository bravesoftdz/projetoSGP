      FieldName = 'IDRECEBER_ITEM'
      Size = 0
    end
    object fdsChequeIDBANCO: TFIBBCDField
      FieldName = 'IDBANCO'
      Size = 0
    end
    object fdsChequeAGENCIA: TFIBStringField
