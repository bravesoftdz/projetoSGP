  object fdsCor: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE COR'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    COR'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO COR('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    ENVIADO_WEB'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :ENVIADO_WEB'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    COR'
      'WHERE( '
      '    STATUS <> '#39'I'#39
      '     ) and (     COR.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    COR'
      'WHERE'
      '    STATUS <> '#39'I'#39)
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Active = True
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 16
    Top = 8
    oFetchAll = True
    object fdsCorID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
