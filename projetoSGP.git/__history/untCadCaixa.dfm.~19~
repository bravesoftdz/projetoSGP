  object qryVerificaCaixa: TpFIBQuery
    Transaction = DM.transPricipal
    Database = DM.Conexao
    SQL.Strings = (
      'SELECT'
      '    NUM_CAIXA'
      'FROM'
      '    CAIXA '
      'WHERE'
      ' NUM_CAIXA=:NUM_CAIXA'
      'AND'
      ' IDFILIAL=:IDFILIAL')
