  object fdsCidade: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '      CL.IDCIDADE,      '
      '      UPPER(TRIM(CI.NOME)) CIDADE,'
      '      CI.UF'
      'FROM'
      '      CLIENTE CL, CIDADE_IBGE CI'
      'WHERE'
      '      CI.ID = CL.IDCIDADE'
      'AND'
      '      CL.ID <> '#39'9999'#39
      'GROUP BY'
      '      CL.IDCIDADE, CIDADE, CI.UF'
      'ORDER BY '
      '      CIDADE'
      '      ')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    AfterOpen = fdsCidadeAfterOpen
    Transaction = DM.transPricipal
