      'SELECT'
      '    ID,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA'
      'FROM'
      '    USUARIO'
      'WHERE( '
      '    LOGIN = :LOGIN'
      'AND'
      '    SENHA = :SENHA'
      '     ) and (     USUARIO.ID = :OLD_ID'
      '     )'
      '     ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA'
      'FROM'
      '    USUARIO'
      'WHERE'
      '    LOGIN = :LOGIN'
      'AND'
      '    SENHA = :SENHA ')
