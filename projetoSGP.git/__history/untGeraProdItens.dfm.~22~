      'SELECT'
      '    T.ID,'
      '    T.NOME,'
      '    T.REFERENCIA'
      'FROM'
      '    TAMANHO T, TAMANHO_GRUPO TG'
      'WHERE( '
      '   TG.STATUS <> '#39'I'#39
      'AND'
      '   T.STATUS <> '#39'I'#39
      'AND'
      '   TG.IDGRUPO = :IDGRUPO '
      'AND '
      '   T.ID = TG.IDTAMANHO'
      '     ) and (     T.ID = :OLD_ID'
      '     )'
      '    ')
