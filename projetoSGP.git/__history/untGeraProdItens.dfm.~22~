      'SELECT'
      '    C.ID,'
      '    C.NOME,'
      '    C.REFERENCIA'
      'FROM'
      '    COR C, COR_GRUPO CG'
      'WHERE'
      '   CG.STATUS <> '#39'I'#39
      'AND'
      '   C.STATUS <> '#39'I'#39
      'AND'
      '   C.ID <> 0'
      'AND'
      '   CG.IDGRUPO = :IDGRUPO '
      'AND '
      '   C.ID = CG.IDCOR')
