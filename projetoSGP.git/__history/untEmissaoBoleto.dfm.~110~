  object fdsConfigBoleto: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CONFIG_BOLETO'
      'SET '
      '    IDBANCO = :IDBANCO,'
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    DT_ULTIMO_RECEBIMENTO = :DT_ULTIMO_RECEBIMENTO,'
      '    PATH_REMESSA = :PATH_REMESSA,'
      '    PATH_RETORNO = :PATH_RETORNO,'
      '    PATH_LOGOTIPO = :PATH_LOGOTIPO,'
      '    PATH_GERARPDF = :PATH_GERARPDF,'
      '    MENSAGEM = :MENSAGEM,'
