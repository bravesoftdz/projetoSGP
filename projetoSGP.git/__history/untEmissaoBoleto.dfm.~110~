      '    INSTRUCAO_2 = :INSTRUCAO_2,'
      '    CONT_REMESSA = :CONT_REMESSA,'
      '    IDFORMA_PAG = :IDFORMA_PAG'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CONFIG_BOLETO'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO CONFIG_BOLETO('
      '    ID,'
      '    IDBANCO,'
      '    IDFILIAL,'
      '    DT_INC,'
      '    DT_ALT,'
      '    DT_ULTIMO_RECEBIMENTO,'
      '    PATH_REMESSA,'
      '    PATH_RETORNO,'
      '    PATH_LOGOTIPO,'
