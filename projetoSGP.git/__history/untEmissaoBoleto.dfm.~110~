      '    ESPECIE,'
      '    MOEDA,'
      '    ACEITE,'
      '    CARTEIRA,'
      '    LOCAL_PAGTO,'
      '    INSTRUCAO_1,'
      '    INSTRUCAO_2,'
      '    CONT_REMESSA,'
      '    IDFORMA_PAG'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDBANCO,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :DT_ULTIMO_RECEBIMENTO,'
      '    :PATH_REMESSA,'
      '    :PATH_RETORNO,'
      '    :PATH_LOGOTIPO,'
      '    :PATH_GERARPDF,'
      '    :MENSAGEM,'
      '    :DIAS_PROTESTO,'
      '    :ESPECIE,'
      '    :MOEDA,'
      '    :ACEITE,'
      '    :CARTEIRA,'
      '    :LOCAL_PAGTO,'
      '    :INSTRUCAO_1,'
      '    :INSTRUCAO_2,'
      '    :CONT_REMESSA,'
      '    :IDFORMA_PAG'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    CONFIG_BOLETO '
      ''
      ' WHERE '
      '        CONFIG_BOLETO.ID = :OLD_ID'
      '    and CONFIG_BOLETO.IDFILIAL = :OLD_IDFILIAL'
      '    ')
