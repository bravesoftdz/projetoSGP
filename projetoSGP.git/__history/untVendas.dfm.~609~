  object fdsNFCe: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA'
      'SET '
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    VLR_VENDA = :VLR_VENDA,'
      '    CODIGO_NFCE = :CODIGO_NFCE,'
      '    SERIE = :SERIE,'
      '    TIPO = :TIPO,'
      '    CHAVE_CONSULTA = :CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO = :NUMERO_PROTOCOLO,'
      '    DATA_EMISSAO = :DATA_EMISSAO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA('
      '    ID,'
      '    VLR_TOTAL,'
      '    VLR_VENDA,'
      '    CODIGO_NFCE,'
      '    SERIE,'
      '    TIPO,'
      '    CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO,'
      '    DATA_EMISSAO'
      ')'
      'VALUES('
      '    :ID,'
      '    :VLR_TOTAL,'
      '    :VLR_VENDA,'
      '    :CODIGO_NFCE,'
      '    :SERIE,'
      '    :TIPO,'
      '    :CHAVE_CONSULTA,'
      '    :NUMERO_PROTOCOLO,'
      '    :DATA_EMISSAO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    V.ID,'
      '    V.VLR_TOTAL,'
      '    V.VLR_VENDA,'
      '    V.CODIGO_NFCE,'
      '    V.SERIE,'
      '    V.TIPO,'
      '    V.CHAVE_CONSULTA,'
      '    V.NUMERO_PROTOCOLO,'
      '    V.DATA_EMISSAO,'
      '    count(VI.ITEM) QTN_ITENS'
      'FROM'
      '    VENDA V,VENDA_ITEM VI'
      'WHERE(  '
      ' V.TIPO='#39'CF'#39
      'AND'
      ' VI.IDVENDA=V.ID'
      'AND'
      '  DATA_EMISSAO between :DT_INICIO AND :DT_FIM'
      '     ) and (     V.ID = :OLD_ID'
      '     )'
      '    '
      
        'GROUP BY V.ID,V.DATA_EMISSAO,V.CODIGO_NFCE,V.SERIE,V.TIPO,V.CHAV' +
        'E_CONSULTA,V.NUMERO_PROTOCOLO,V.VLR_TOTAL, V.VLR_VENDA')
    SelectSQL.Strings = (
      'SELECT'
      '    V.ID,'
      '    V.VLR_TOTAL,'
      '    V.VLR_VENDA,'
      '    V.CODIGO_NFCE,'
      '    V.SERIE,'
      '    V.TIPO,'
      '    V.CHAVE_CONSULTA,'
      '    V.NUMERO_PROTOCOLO,'
      '    V.DATA_EMISSAO,'
      '    count(VI.ITEM) QTN_ITENS,'
      
        '    CASE V.TIPO WHEN '#39'CF'#39' THEN '#39'Autorizado'#39' WHEN '#39'OF'#39' THEN '#39'Cont' +
        'ig'#234'ncia'#39' END AS STATUS'
      'FROM'
      '    VENDA V,VENDA_ITEM VI'
      'WHERE '
      ' (V.TIPO='#39'CF'#39' OR (V.TIPO='#39'OF'#39'))'
      'AND'
      ' VI.IDVENDA=V.ID'
      'AND'
      'V.IDFILIAL=:IDFILIAL'
      'AND'
      '  V.DATA_EMISSAO between :DTPINICIO AND :DTPFIM'
      
        'GROUP BY V.ID,V.DATA_EMISSAO,V.CODIGO_NFCE,V.SERIE,V.TIPO,V.CHAV' +
        'E_CONSULTA,V.NUMERO_PROTOCOLO,V.VLR_TOTAL, V.VLR_VENDA'
      'order BY V.DATA_EMISSAO')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 441
    Top = 280
