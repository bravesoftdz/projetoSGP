  object fdsVenda: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA'
      'SET '
      '    IDCAIXA = :IDCAIXA,'
      '    DT_INC = :DT_INC,'
      '    STATUS = :STATUS,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    IDVENDEDOR = :IDVENDEDOR,'
      '    IDFORMA_PAG = :IDFORMA_PAG,'
      '    VLR_TOTITEM = :VLR_TOTITEM,'
      '    VLR_DESCONTO = :VLR_DESCONTO,'
      '    VLR_ENTRADA = :VLR_ENTRADA,'
      '    VLR_FRETE = :VLR_FRETE,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    VLR_VENDA = :VLR_VENDA,'
      '    PESO = :PESO,'
      '    PREFIXO = :PREFIXO,'
      '    DT_FECHAMENTO = :DT_FECHAMENTO,'
      '    SUBTRIBUTARIA = :SUBTRIBUTARIA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    ENTREGAR = :ENTREGAR,'
      '    OBS = :OBS,'
      '    TOT_PARCELA = :TOT_PARCELA,'
      '    VLR_RETORNADO = :VLR_RETORNADO,'
      '    DT_PAGAMENTO = :DT_PAGAMENTO,'
      '    VISUALIZADO = :VISUALIZADO,'
      '    NUM_ALEATORIO_NFCE = :NUM_ALEATORIO_NFCE,'
      '    CODIGO_NFCE = :CODIGO_NFCE,'
      '    SERIE = :SERIE,'
      '    TIPO = :TIPO,'
      '    CHAVE_CONSULTA = :CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO = :NUMERO_PROTOCOLO,'
      '    DATA_EMISSAO = :DATA_EMISSAO,'
      '    TIPO_DESC_ACRESC = :TIPO_DESC_ACRESC'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA('
      '    ID,'
      '    IDFILIAL,'
      '    IDCAIXA,'
      '    DT_INC,'
      '    STATUS,'
      '    IDCLIENTE,'
      '    IDVENDEDOR,'
      '    IDFORMA_PAG,'
      '    VLR_TOTITEM,'
      '    VLR_DESCONTO,'
      '    VLR_ENTRADA,'
      '    VLR_FRETE,'
      '    VLR_TOTAL,'
      '    VLR_VENDA,'
      '    PESO,'
      '    PREFIXO,'
      '    DT_FECHAMENTO,'
      '    SUBTRIBUTARIA,'
      '    ENVIADO_WEB,'
      '    ENTREGAR,'
      '    OBS,'
      '    TOT_PARCELA,'
      '    VLR_RETORNADO,'
      '    DT_PAGAMENTO,'
      '    VISUALIZADO,'
      '    NUM_ALEATORIO_NFCE,'
      '    CODIGO_NFCE,'
      '    SERIE,'
      '    TIPO,'
      '    CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO,'
      '    DATA_EMISSAO,'
      '    TIPO_DESC_ACRESC'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :IDCAIXA,'
      '    :DT_INC,'
      '    :STATUS,'
      '    :IDCLIENTE,'
      '    :IDVENDEDOR,'
      '    :IDFORMA_PAG,'
      '    :VLR_TOTITEM,'
      '    :VLR_DESCONTO,'
      '    :VLR_ENTRADA,'
      '    :VLR_FRETE,'
      '    :VLR_TOTAL,'
      '    :VLR_VENDA,'
      '    :PESO,'
      '    :PREFIXO,'
      '    :DT_FECHAMENTO,'
      '    :SUBTRIBUTARIA,'
      '    :ENVIADO_WEB,'
      '    :ENTREGAR,'
      '    :OBS,'
      '    :TOT_PARCELA,'
      '    :VLR_RETORNADO,'
      '    :DT_PAGAMENTO,'
      '    :VISUALIZADO,'
      '    :NUM_ALEATORIO_NFCE,'
      '    :CODIGO_NFCE,'
      '    :SERIE,'
      '    :TIPO,'
      '    :CHAVE_CONSULTA,'
      '    :NUMERO_PROTOCOLO,'
      '    :DATA_EMISSAO,'
      '    :TIPO_DESC_ACRESC'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    V.*,'
      
        '    case v.status  when '#39'AB'#39' then '#39'C'#39' when '#39'CA'#39' then '#39'C'#39' when '#39'C' +
        'F'#39' then '#39'A'#39' when '#39'FC'#39' then '#39'B'#39' when '#39'DV'#39' then '#39'B'#39' end AS ORDEM,'
      '    F.NOME FILIAL,'
      '    C.NOME AS CLIENTE,'
      '    C.CONTATO,'
      '    C.RAZAOSOCIAL,'
      '    C.EMAIL,'
      '    C.ENDERECO,'
      '    C.CEP,'
      '    C.BAIRRO,'
      '    C.REFERENCIA,'
      '    C.TELEFONE,'
      '    C.CELULAR,'
      '    C.KM_TOTAL,'
      '    CD.NOME || '#39' - '#39' || CD.UF AS CIDADE,'
      
        '    (CASE c.TIPO WHEN '#39'F'#39' then Trim(C.CPF) WHEN '#39'J'#39' then Trim(C.' +
        'cnpj) END) AS DOCUMENTO,'
      '    R.NOME ROTA,'
      '    FP.NOME FORMA_PAG,'
      '    VE.NOME VENDEDOR'
      'FROM'
      
        '    VENDA V, FILIAL F, CLIENTE C, FORMA_PAGAMENTO FP, VENDEDOR V' +
        'E, ROTA R, CIDADE_IBGE CD'
      'WHERE( '
      '    F.ID = V.IDFILIAL'
      'AND'
      '    C.ID = V.IDCLIENTE'
      'AND'
      '    CD.ID = C.IDCIDADE'
      'AND '
      '    FP.ID = V.IDFORMA_PAG'
      'AND'
      '    VE.ID = V.IDVENDEDOR'
      'AND'
      '    R.ID = C.IDROTA'
      '     ) and (     V.ID = :OLD_ID'
      '    and V.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    '
      ''
      ''
      ''
      '')
    SelectSQL.Strings = (
      'SELECT'
      '    V.*,'
      
        '    case v.status  when '#39'AB'#39' then '#39'C'#39' when '#39'CA'#39' then '#39'C'#39' when '#39'C' +
        'F'#39' then '#39'A'#39' when '#39'FC'#39' then '#39'B'#39' when '#39'DV'#39' then '#39'B'#39' end AS ORDEM,'
      '    F.NOME FILIAL,'
      '    C.NOME AS CLIENTE,'
      '    C.CONTATO,'
      '    C.RAZAOSOCIAL,'
      '    C.EMAIL,'
      '    C.ENDERECO,'
      '    C.CEP,'
      '    C.BAIRRO,'
      '    C.REFERENCIA,'
      '    C.TELEFONE,'
      '    C.CELULAR,'
      '    C.KM_TOTAL,'
      '    CD.NOME || '#39' - '#39' || CD.UF AS CIDADE,'
      
        '    (CASE c.TIPO WHEN '#39'F'#39' then Trim(C.CPF) WHEN '#39'J'#39' then Trim(C.' +
        'cnpj) END) AS DOCUMENTO,'
      '    R.NOME ROTA,'
      '    FP.NOME FORMA_PAG,'
      '    VE.NOME VENDEDOR'
      'FROM'
      
        '    VENDA V, FILIAL F, CLIENTE C, FORMA_PAGAMENTO FP, VENDEDOR V' +
        'E, ROTA R, CIDADE_IBGE CD'
      'WHERE'
      '    F.ID = V.IDFILIAL'
      'AND'
      '    C.ID = V.IDCLIENTE'
      'AND'
      '    CD.ID = C.IDCIDADE'
      'AND '
      '    FP.ID = V.IDFORMA_PAG'
      'AND'
      '    VE.ID = V.IDVENDEDOR'
      'AND'
      '    R.ID = C.IDROTA'
      ''
      ''
      ''
      '')
    AfterScroll = fdsVendaAfterScroll
    AllowedUpdateKinds = [ukModify]
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 616
    oFetchAll = True
