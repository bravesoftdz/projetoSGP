  object fdsTodasVendasFormaPag: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '    FORMA_PAG,'
      
        '    CASE FORMA_PAG WHEN '#39'1'#39' THEN '#39'Dinheiro'#39' WHEN '#39'2'#39'THEN '#39'Cart'#227'o' +
        ' de Cr'#233'dito'#39' WHEN '#39'3'#39'THEN '#39'Cart'#227'o de d'#233'bito'#39'WHEN '#39'4'#39'THEN '#39'Cheque' +
        #39'WHEN '#39'5'#39'THEN '#39'Financeira'#39'WHEN '#39'6'#39'THEN '#39'Duplicata'#39'WHEN '#39'7'#39'THEN '#39 +
        'Vale Desconto'#39'WHEN '#39'8'#39'THEN '#39'Ticket'#39'WHEN '#39'9'#39'THEN '#39'Cr'#233'dito'#39'WHEN '#39'1' +
        '0'#39'THEN '#39'Boleto'#39'WHEN '#39'11'#39'THEN '#39'Nenhum'#39' END As NOME_FORMA_PAG,   '
      '    SUM(QTN)AS QTN,'
      '    SUM(VLR_TOTAL)AS VLR_TOTAL,'
      '    PREFIXO'
      'FROM'
      '    V_TODAS_VENDAS_FORMA_PAG '
      'WHERE'
      ' DT_INC BETWEEN :DTPINICIO AND :DTPFIM '
      ' GROUP BY PREFIXO,FORMA_PAG')
