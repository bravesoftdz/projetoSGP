  object fdsNFe: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE NFE'
      'SET '
      '    DT_EMISSAO = :DT_EMISSAO,'
      '    STATUS = :STATUS,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    CODIGO_NFE = :CODIGO_NFE,'
      '    SERIE = :SERIE,'
      '    CHAVE_CONSULTA = :CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO = :NUMERO_PROTOCOLO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    NFE'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO NFE('
      '    ID,'
      '    DT_EMISSAO,'
      '    STATUS,'
      '    VLR_TOTAL,'
      '    CODIGO_NFE,'
      '    SERIE,'
      '    CHAVE_CONSULTA,'
      '    NUMERO_PROTOCOLO'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_EMISSAO,'
      '    :STATUS,'
      '    :VLR_TOTAL,'
      '    :CODIGO_NFE,'
      '    :SERIE,'
      '    :CHAVE_CONSULTA,'
      '    :NUMERO_PROTOCOLO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    N.ID,'
      '    N.DT_EMISSAO,'
      '    N.STATUS,'
      '    N.VLR_TOTAL,'
      '    N.CODIGO_NFE,'
      '    N.SERIE,'
      '    N.CHAVE_CONSULTA,'
      '    N.NUMERO_PROTOCOLO,'
      '    count(NI.id) QTN_ITENS'
      'FROM'
      '    NFE N,NFE_ITEM NI'
      'WHERE( '
      ' N.STATUS='#39'NF'#39
      'AND'
      ' NI.IDNFE= N.ID'
      'AND'
      ' N.IDFILIAL=:IDFILIAL'
      'AND'
      '  N.DT_EMISSAO between :DT_INICIO AND :DT_FIM'
      '     ) and (     N.ID = :OLD_ID'
      '     )'
      '    '
      ''
      
        'GROUP BY N.ID,N.CODIGO_NFE,N.DT_EMISSAO,N.STATUS,N.VLR_TOTAL,N.S' +
        'ERIE,N.CHAVE_CONSULTA, N.NUMERO_PROTOCOLO')
    SelectSQL.Strings = (
      'SELECT'
      '    N.ID,'
      '    N.DT_EMISSAO,'
      '    N.STATUS,'
      '    N.VLR_TOTAL,'
      '    N.CODIGO_NFE,'
      '    N.SERIE,'
      '    N.CHAVE_CONSULTA,'
      '    N.NUMERO_PROTOCOLO,'
      '    count(NI.id) QTN_ITENS,'
      
        '    CASE N.STATUS WHEN '#39'NF'#39' THEN '#39'Autorizado'#39' WHEN '#39'OF'#39' THEN '#39'Co' +
        'ntig'#234'ncia'#39' END AS STATUS_NF '
      'FROM'
      '    NFE N,NFE_ITEM NI'
      'WHERE'
      ' (N.STATUS='#39'NF'#39'OR (N.STATUS='#39'OF'#39'))'
      'AND'
      ' NI.IDNFE= N.ID'
      'AND'
      ' N.IDFILIAL=:IDFILIAL'
      'AND'
      '  N.DT_EMISSAO between :DTPINICIO AND :DTPFIM'
      ''
      
        'GROUP BY N.ID,N.CODIGO_NFE,N.DT_EMISSAO,N.STATUS,N.VLR_TOTAL,N.S' +
        'ERIE,N.CHAVE_CONSULTA, N.NUMERO_PROTOCOLO'
      'order BY N.DT_EMISSAO')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 369
    Top = 280
