  object fdsVendaItem: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA_ITEM'
      'SET '
      '    IDPROD_ITEM = :IDPROD_ITEM,'
      '    DT_INC = :DT_INC,'
      '    QTN = :QTN,'
      '    VLR_UNITARIO = :VLR_UNITARIO,'
      '    VLR_DESC_ACRE = :VLR_DESC_ACRE,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    PESO = :PESO,'
      '    QTN_RETORNO = :QTN_RETORNO,'
      '    MOTIVO_RETORNO = :MOTIVO_RETORNO,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    ENTREGAR = :ENTREGAR,'
      '    TIPO_DESC_ACRE = :TIPO_DESC_ACRE'
      'WHERE'
      '    IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA_ITEM'
      'WHERE'
      '        IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA_ITEM('
      '    IDVENDA,'
      '    ITEM,'
      '    IDPROD_ITEM,'
      '    DT_INC,'
      '    QTN,'
      '    VLR_UNITARIO,'
      '    VLR_DESC_ACRE,'
      '    VLR_TOTAL,'
      '    PESO,'
      '    QTN_RETORNO,'
      '    MOTIVO_RETORNO,'
      '    ENVIADO_WEB,'
      '    IDFILIAL,'
      '    ENTREGAR,'
      '    TIPO_DESC_ACRE'
      ')'
      'VALUES('
      '    :IDVENDA,'
      '    :ITEM,'
      '    :IDPROD_ITEM,'
      '    :DT_INC,'
      '    :QTN,'
      '    :VLR_UNITARIO,'
      '    :VLR_DESC_ACRE,'
      '    :VLR_TOTAL,'
      '    :PESO,'
      '    :QTN_RETORNO,'
      '    :MOTIVO_RETORNO,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL,'
      '    :ENTREGAR,'
      '    :TIPO_DESC_ACRE'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    VI.*,'
      '    CASE WHEN (VI.TIPO_DESC_ACRE = '#39'A'#39') THEN '#39'Acr'#233'scimo'#39
      
        '         WHEN (VI.TIPO_DESC_ACRE = '#39'D'#39') THEN '#39'Desconto'#39' END AS T' +
        'P,'
      '    PI.NOME PRODUTO,'
      '    PI.REFERENCIA'
      'FROM'
      '    VENDA_ITEM VI, PRODUTO_ITEM PI'
      'WHERE( '
      '    PI.ID = VI.IDPROD_ITEM'
      '     ) and (     VI.IDVENDA = :OLD_IDVENDA'
      '    and VI.ITEM = :OLD_ITEM'
      '    and VI.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    VI.*,'
      '    CASE WHEN (VI.TIPO_DESC_ACRE = '#39'A'#39') THEN '#39'Acr'#233'scimo'#39
      
        '         WHEN (VI.TIPO_DESC_ACRE = '#39'D'#39') THEN '#39'Desconto'#39' END AS T' +
        'P,'
      '    PI.NOME PRODUTO,'
      '    PI.REFERENCIA'
      'FROM'
      '    VENDA_ITEM VI, PRODUTO_ITEM PI'
      'WHERE'
      '    PI.ID = VI.IDPROD_ITEM')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 840
    Top = 56
    oFetchAll = True
