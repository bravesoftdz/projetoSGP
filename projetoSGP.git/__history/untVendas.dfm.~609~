    SelectSQL.Strings = (
      'SELECT'
      
        '    SUM(MC.vlr_entrada) - (SELECT SUM(T.TROCO) FROM MOVIMENTO_CA' +
        'IXA T WHERE T.dt_inc between :DTINICIO AND :DTFIM)) DN,'
      '    SUM(MC.vlr_cheque) CH,'
      '    SUM(MC.vlr_cartao) CT,'
      '    SUM(MC.vlr_nota) NT,'
      '    SUM(MC.vlr_financeira) FN,'
      '    SUM(MC.vlr_valedesconto) VD,'
      '    SUM(MC.vlr_ticket) TK'
      ''
      'FROM '
      '    MOVIMENTO_CAIXA MC'
      'WHERE'
      '   MC.TIPO = '#39'VE'#39
