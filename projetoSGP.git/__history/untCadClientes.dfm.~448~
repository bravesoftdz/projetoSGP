  object fdsRazaoProdMes: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      
        '     CAST(EXTRACT (MONTH FROM dt_inc) || '#39'/'#39' || '#39'01'#39' || '#39'/'#39' || E' +
        'XTRACT (YEAR FROM dt_inc) as date) AS DT,'
      '     idprod_item, ean, nomeproduto, sum(qtn) QTN'
      'FROM '
      '     V_RAZAO_CLIENTE_PRODUTO VRCP'
      'group by dt, idprod_item, ean, nomeproduto'
      'order by dt')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
