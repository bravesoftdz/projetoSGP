      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :IDPRODUTO,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDTAMANHO,'
      '    :IDCOR,'
      '    :PESO,'
      '    :PREC_CUSTO,'
      '    :PREC_CUSTO_MEDIO,'
      '    :COD_FABRICA,'
      '    :PREC_ULTIMO_CUSTO,'
      '    :DT_VALIDADE,'
      '    :DESC_MAX1,'
      '    :DESC_MAX2,'
      '    :DESC_MAX3,'
      '    :DESC_MAX4,'
      '    :COD_FABRICANTE,'
      '    :COMISSAO_DN,'
      '    :COMISSAO_CH,'
      '    :COMISSAO_CT,'
      '    :UNIDADE,'
      '    :ENVIADO_WEB,'
      '    :UTILIZA_BALANCA,'
      '    :OBS,'
      '    :IDENTIFICADOR,'
      '    :INFO_COMPLEMENTAR'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PI.*,'
      '    P.CFOP,'
      '    P.NCM,'
      '    P.TIPO,'
      '    P.INFO_ADICIONAL,'
      '    P.CSOSN,'
      '    P.ALIQ_ICMS,'
      '    P.CEST,'
      '    P.FCP,'
      '    P.CST_PIS,'
      '    P.CST_COFINS,'
      '    PIE.*'
      'FROM'
      '    PRODUTO_ITEM PI, PRODUTO P, PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE( '
      '    P.ID = PI.IDPRODUTO'
      'AND'
      '    PIE.IDITEM = PI.ID'
      'AND'
      '    PIE.IDFILIAL = :IDFILIAL'
      '     ) and (     PI.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PI.*,'
      '    P.CFOP,'
      '    P.NCM,'
      '    P.TIPO,'
      '    P.INFO_ADICIONAL,'
      '    P.CSOSN,'
      '    P.ALIQ_ICMS,'
      '    P.CEST,'
      '    P.FCP,'
      '    P.CST_PIS,'
      '    P.CST_COFINS,'
      '    PIE.*'
      'FROM'
      '    PRODUTO_ITEM PI, PRODUTO P, PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE'
      '    P.ID = PI.IDPRODUTO'
      'AND'
      '    PIE.IDITEM = PI.ID'
      'AND'
      '    PIE.IDFILIAL = :IDFILIAL'
      'ORDER BY '
      '    NOME')
    BeforeOpen = fdsProdutoItensBeforeOpen
