  object fdsPlanoContas: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PLANO_CONTAS'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PLANO_CONTAS'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PLANO_CONTAS('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    PLANO_CONTAS '
      ''
      ' WHERE '
      '        PLANO_CONTAS.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    PLANO_CONTAS ')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 320
    Top = 8
    oFetchAll = True
    object fdsPlanoContasID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
    object fdsPlanoContasDT_INC: TFIBDateField
      FieldName = 'DT_INC'
    end
    object fdsPlanoContasDT_ALT: TFIBDateField
      FieldName = 'DT_ALT'
    end
    object fdsPlanoContasSTATUS: TFIBStringField
      FieldName = 'STATUS'
      Size = 1
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsPlanoContasNOME: TFIBStringField
      FieldName = 'NOME'
      Size = 200
      Transliterate = False
      EmptyStrToNull = True
    end
  end
  object dsPlanoContas: TDataSource
    AutoEdit = False
    DataSet = fdsPlanoContas
    Left = 384
    Top = 8
