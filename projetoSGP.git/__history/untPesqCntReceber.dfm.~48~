  object fdsCntReceberItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CNT_RECEBER_ITEM'
      'SET '
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDRECEBER = :IDRECEBER,'
      '    DT_VENC = :DT_VENC,'
      '    DT_PAG = :DT_PAG,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
      '    HISTORICO = :HISTORICO,'
      '    PARCELA = :PARCELA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    BAIXA_AGRUPADA = :BAIXA_AGRUPADA,'
      '    PAGAMENTO_MOEDA = :PAGAMENTO_MOEDA,'
      '    REC_DESC_ACRE = :REC_DESC_ACRE,'
      '    REC_DINHEIRO = :REC_DINHEIRO,'
      '    ID_REC_ITEM = :ID_REC_ITEM,'
      '    VALOR_JUROS_PAGO = :VALOR_JUROS_PAGO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CNT_RECEBER_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO CNT_RECEBER_ITEM('
      '    ID,'
      '    POSICAO,'
      '    PREFIXO,'
      '    REFERENCIA,'
      '    IDRECEBER,'
      '    DT_VENC,'
      '    DT_PAG,'
      '    ACRESCIMO,'
      '    DESCONTO,'
      '    VALOR,'
      '    VALOR_PAGO,'
      '    HISTORICO,'
      '    PARCELA,'
      '    ENVIADO_WEB,'
      '    IDFILIAL,'
      '    BAIXA_AGRUPADA,'
      '    PAGAMENTO_MOEDA,'
      '    REC_DESC_ACRE,'
      '    REC_DINHEIRO,'
      '    ID_REC_ITEM,'
      '    VALOR_JUROS_PAGO'
      ')'
      'VALUES('
      '    :ID,'
      '    :POSICAO,'
      '    :PREFIXO,'
      '    :REFERENCIA,'
      '    :IDRECEBER,'
      '    :DT_VENC,'
      '    :DT_PAG,'
      '    :ACRESCIMO,'
      '    :DESCONTO,'
      '    :VALOR,'
      '    :VALOR_PAGO,'
      '    :HISTORICO,'
      '    :PARCELA,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL,'
      '    :BAIXA_AGRUPADA,'
      '    :PAGAMENTO_MOEDA,'
      '    :REC_DESC_ACRE,'
      '    :REC_DINHEIRO,'
      '    :ID_REC_ITEM,'
      '    :VALOR_JUROS_PAGO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    CRI.*,'
      '    CL.NOME CLIENTE'
      'FROM'
      '    CNT_RECEBER_ITEM CRI, CNT_RECEBER CR, CLIENTE CL'
      'WHERE( '
      '    CR.ID = CRI.IDRECEBER'
      'AND'
      '    CRI.POSICAO = '#39'AB'#39
      'AND'
      '    CL.ID = CR.IDCLIENTE'
      '     ) and (     CRI.ID = :OLD_ID'
      '    and CRI.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    CRI.*,'
      '    CR.IDVENDA,'
      '    CL.NOME CLIENTE'
      'FROM'
      '    CNT_RECEBER_ITEM CRI, CNT_RECEBER CR, CLIENTE CL'
      'WHERE'
      
        '    NOT EXISTS (SELECT RCRI.* FROM rom_cntreceber_item RCRI, rom' +
        'aneio_cntreceber RCR WHERE RCRI.IDRECITEM = CRI.IDRECEBER AND RC' +
        'RI.IDFILIAL = CRI.IDFILIAL AND RCRI.VLR_RECEBER <= RCRI.VLR_RECE' +
        'BIDO AND RCR.id = RCRI.idromaneio AND RCR.status = '#39'FC'#39')'
      'AND'
      
        '        NOT EXISTS (SELECT RCRI.* FROM rom_cntreceber_item RCRI,' +
        ' romaneio_cntreceber RCR WHERE RCRI.IDRECITEM = CRI.IDRECEBER AN' +
        'D RCRI.IDFILIAL = CRI.IDFILIAL AND RCR.id = RCRI.idromaneio AND ' +
        'RCR.status = '#39'AB'#39')'
      'AND'
      '    CR.ID = CRI.IDRECEBER'
      'AND'
      '    CRI.POSICAO = '#39'AB'#39
      'AND'
      '    CL.ID = CR.IDCLIENTE'
      'AND'
      '    ((CRI.PREFIXO = '#39'NT'#39' OR CRI.PREFIXO = '#39'CH'#39'))'
      'ORDER BY CRI.DT_VENC, ID'
      '    ')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 32
    Top = 8
    oFetchAll = True
