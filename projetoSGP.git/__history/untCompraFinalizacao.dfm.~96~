    UpdateSQL.Strings = (
      'UPDATE CNT_PAGAR_ITEM'
      'SET '
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDPAGAR = :IDPAGAR,'
      '    DT_VENC = :DT_VENC,'
      '    DT_PAG = :DT_PAG,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
      '    HISTORICO = :HISTORICO,'
      '    PARCELA = :PARCELA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    NUM_DOCUMENTO = :NUM_DOCUMENTO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CNT_PAGAR_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
