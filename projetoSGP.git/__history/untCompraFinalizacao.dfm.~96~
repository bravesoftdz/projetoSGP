    Database = DM.Conexao
    SQL.Strings = (
      'SELECT '
      '     ID '
      'FROM '
      '     CNT_PAGAR_ITEM'
      'WHERE'
      '     IDPAGAR = :IDPAGAR'
      'AND'
      '     IDFILIAL = :IDFILIAL'
      'AND'
      '     DT_VENC = :DT_VENC'
      'AND'
      '     VALOR = :VALOR')
    Left = 88
    Top = 56
  end
