  object fdsPreVendaItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PREVENDA_ITEM'
      'SET '
      '    IDPROD_ITEM = :IDPROD_ITEM,'
      '    DT_INC = :DT_INC,'
      '    QTN = :QTN,'
      '    VLR_UNITARIO = :VLR_UNITARIO,'
      '    VLR_DESC = :VLR_DESC,'
      '    VLR_TOTAL = :VLR_TOTAL'
      'WHERE'
      '    IDPREVENDA = :OLD_IDPREVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PREVENDA_ITEM'
      'WHERE'
      '        IDPREVENDA = :OLD_IDPREVENDA'
      '    and ITEM = :OLD_ITEM'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PREVENDA_ITEM('
      '    IDPREVENDA,'
      '    ITEM,'
      '    IDPROD_ITEM,'
      '    DT_INC,'
      '    QTN,'
      '    VLR_UNITARIO,'
      '    VLR_DESC,'
      '    VLR_TOTAL,'
      '    IDFILIAL'
      ')'
      'VALUES('
      '    :IDPREVENDA,'
      '    :ITEM,'
      '    :IDPROD_ITEM,'
      '    :DT_INC,'
      '    :QTN,'
      '    :VLR_UNITARIO,'
      '    :VLR_DESC,'
      '    :VLR_TOTAL,'
      '    :IDFILIAL'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PVI.*,'
      '    I.NOME,'
      '    I.COD_FABRICA'
      'FROM'
      '    PREVENDA_ITEM PVI, PRODUTO_ITEM I'
      'WHERE( '
      '   I.ID = PVI.IDPROD_ITEM'
      '     ) and (     PVI.IDPREVENDA = :OLD_IDPREVENDA'
      '    and PVI.ITEM = :OLD_ITEM'
      '    and PVI.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PVI.*,'
      '    I.NOME,'
      '    I.COD_FABRICA'
      'FROM'
      '    PREVENDA_ITEM PVI, PRODUTO_ITEM I'
      'WHERE'
      '   I.ID = PVI.IDPROD_ITEM')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 624
  end
