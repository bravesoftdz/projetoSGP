  object fdsPreVenda: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PREVENDA'
      'SET '
      '    DT_INC = :DT_INC,'
      '    STATUS = :STATUS,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    IDVENDEDOR = :IDVENDEDOR,'
      '    IDFORMA_PAG = :IDFORMA_PAG,'
      '    VLR_TOTITEM = :VLR_TOTITEM,'
      '    VLR_DESCONTO = :VLR_DESCONTO,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    VLR_VENDA = :VLR_VENDA,'
      '    PREFIXO = :PREFIXO,'
      '    NUM_PROPIO = :NUM_PROPIO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PREVENDA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PREVENDA('
      '    ID,'
      '    IDFILIAL,'
      '    DT_INC,'
      '    STATUS,'
      '    IDCLIENTE,'
      '    IDVENDEDOR,'
      '    IDFORMA_PAG,'
      '    VLR_TOTITEM,'
      '    VLR_DESCONTO,'
      '    VLR_TOTAL,'
      '    VLR_VENDA,'
      '    PREFIXO,'
      '    NUM_PROPIO'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :STATUS,'
      '    :IDCLIENTE,'
      '    :IDVENDEDOR,'
      '    :IDFORMA_PAG,'
      '    :VLR_TOTITEM,'
      '    :VLR_DESCONTO,'
      '    :VLR_TOTAL,'
      '    :VLR_VENDA,'
      '    :PREFIXO,'
      '    :NUM_PROPIO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PV.*,'
      '    V.NOME VENDEDOR,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAGAMENTO'
      'FROM'
      '    PREVENDA PV, VENDEDOR V, CLIENTE C, FORMA_PAGAMENTO FP'
      'WHERE( '
      '    V.ID = PV.IDVENDEDOR'
      'AND'
      '    C.ID = PV.IDCLIENTE'
      'AND'
      '    FP.ID = PV.IDFORMA_PAG'
      '     ) and (     PV.ID = :OLD_ID'
      '    and PV.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PV.*,'
      '    V.NOME VENDEDOR,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAGAMENTO'
      'FROM'
      '    PREVENDA PV, VENDEDOR V, CLIENTE C, FORMA_PAGAMENTO FP'
      'WHERE'
      '    V.ID = PV.IDVENDEDOR'
      'AND'
      '    C.ID = PV.IDCLIENTE'
      'AND'
      '    FP.ID = PV.IDFORMA_PAG'
      'ORDER BY'
      '    PV.ID DESC')
    AutoUpdateOptions.UpdateTableName = 'PREVENDA'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_PREVENDA'
    AutoUpdateOptions.WhenGetGenID = wgOnNewRecord
    AfterScroll = fdsPreVendaAfterScroll
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 544
    object fdsPreVendaID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
    object fdsPreVendaIDFILIAL: TFIBBCDField
      FieldName = 'IDFILIAL'
      Size = 0
    end
    object fdsPreVendaDT_INC: TFIBDateField
      FieldName = 'DT_INC'
    end
    object fdsPreVendaSTATUS: TFIBStringField
      FieldName = 'STATUS'
      Size = 2
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsPreVendaIDCLIENTE: TFIBBCDField
      FieldName = 'IDCLIENTE'
      Size = 0
    end
    object fdsPreVendaIDVENDEDOR: TFIBBCDField
      FieldName = 'IDVENDEDOR'
      Size = 0
    end
    object fdsPreVendaIDFORMA_PAG: TFIBBCDField
      FieldName = 'IDFORMA_PAG'
      Size = 0
    end
    object fdsPreVendaVLR_TOTITEM: TFIBBCDField
