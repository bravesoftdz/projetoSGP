      'UPDATE COR_GRUPO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS'
      'WHERE'
      '    IDCOR = :OLD_IDCOR'
      '    and IDGRUPO = :OLD_IDGRUPO'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    COR_GRUPO'
      'WHERE'
      '        IDCOR = :OLD_IDCOR'
      '    and IDGRUPO = :OLD_IDGRUPO'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO COR_GRUPO('
      '    IDCOR,'
      '    IDGRUPO,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS'
      ')'
      'VALUES('
      '    :IDCOR,'
      '    :IDGRUPO,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    IDCOR,'
      '    IDGRUPO,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS'
      'FROM'
      '    COR_GRUPO '
      ''
      ' WHERE '
      '        COR_GRUPO.IDCOR = :OLD_IDCOR'
      '    and COR_GRUPO.IDGRUPO = :OLD_IDGRUPO'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    CR.IDCOR,'
      '    CR.IDGRUPO,'
      '    CR.DT_INC,'
      '    CR.DT_ALT,'
      '    CR.STATUS,'
      
        '    case CR.status  when '#39'A'#39' then '#39'Ativo'#39' when '#39'I'#39' then '#39'inativo' +
        #39' end AS ST,'
      '    G.NOME AS GRUPO'
      'FROM'
      '    COR_GRUPO CR, GRUPO G'
      'WHERE'
      '    IDCOR = :IDCOR '
      'AND'
      '    G.ID = IDGRUPO ')
