    end
    object cdsTamanhoREFERENCIA: TStringField
      FieldName = 'REFERENCIA'
    end
    object cdsTamanhoSELECT: TStringField
      FieldKind = fkCalculated
      FieldName = 'SELECT'
      Calculated = True
    end
  end
end
