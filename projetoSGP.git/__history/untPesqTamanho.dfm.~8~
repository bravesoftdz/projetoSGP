  object fdsTamanho: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE TAMANHO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    ALTURA_I = :ALTURA_I,'
      '    LARGURA_I = :LARGURA_I,'
      '    PROFUNDIDADE_I = :PROFUNDIDADE_I,'
      '    ALTURA_E = :ALTURA_E,'
      '    LARGURA_E = :LARGURA_E,'
      '    PROFUNDIDADE_E = :PROFUNDIDADE_E,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    TAMANHO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO TAMANHO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    ALTURA_I,'
      '    LARGURA_I,'
      '    PROFUNDIDADE_I,'
      '    ALTURA_E,'
      '    LARGURA_E,'
      '    PROFUNDIDADE_E,'
      '    ENVIADO_WEB'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :ALTURA_I,'
      '    :LARGURA_I,'
      '    :PROFUNDIDADE_I,'
      '    :ALTURA_E,'
      '    :LARGURA_E,'
      '    :PROFUNDIDADE_E,'
      '    :ENVIADO_WEB'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    TAMANHO'
      'WHERE( '
      '    STATUS <> '#39'I'#39
      '     ) and (     TAMANHO.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    TAMANHO'
      'WHERE'
      '    STATUS <> '#39'I'#39)
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Active = True
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 16
    Top = 8
    oFetchAll = True
