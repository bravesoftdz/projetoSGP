  object fdsItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PRODUTO_ITEM'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    IDPRODUTO = :IDPRODUTO,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
