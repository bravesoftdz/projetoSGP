      '    UNIDADE = :UNIDADE,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    UTILIZA_BALANCA = :UTILIZA_BALANCA'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PRODUTO_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO_ITEM('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    IDPRODUTO,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDTAMANHO,'
      '    IDCOR,'
      '    PESO,'
      '    PREC_CUSTO,'
      '    PREC_CUSTO_MEDIO,'
      '    COD_FABRICA,'
      '    PREC_ULTIMO_CUSTO,'
      '    DT_VALIDADE,'
      '    DESC_MAX1,'
      '    DESC_MAX2,'
      '    DESC_MAX3,'
      '    DESC_MAX4,'
      '    COD_FABRICANTE,'
      '    COMISSAO_DN,'
      '    COMISSAO_CH,'
      '    COMISSAO_CT,'
      '    UNIDADE,'
      '    ENVIADO_WEB,'
      '    UTILIZA_BALANCA'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :IDPRODUTO,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDTAMANHO,'
      '    :IDCOR,'
      '    :PESO,'
      '    :PREC_CUSTO,'
      '    :PREC_CUSTO_MEDIO,'
      '    :COD_FABRICA,'
      '    :PREC_ULTIMO_CUSTO,'
      '    :DT_VALIDADE,'
      '    :DESC_MAX1,'
      '    :DESC_MAX2,'
      '    :DESC_MAX3,'
      '    :DESC_MAX4,'
      '    :COD_FABRICANTE,'
      '    :COMISSAO_DN,'
      '    :COMISSAO_CH,'
      '    :COMISSAO_CT,'
      '    :UNIDADE,'
      '    :ENVIADO_WEB,'
      '    :UTILIZA_BALANCA'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PI.*,'
      '    T.NOME TAMANHO,'
      '    T.ALTURA_I,'
      '    T.LARGURA_I,'
      '    T.PROFUNDIDADE_I,'
      '    T.ALTURA_E,'
      '    T.LARGURA_E,'
      '    T.PROFUNDIDADE_E,'
      '    C.NOME COR,'
      '    PIE.SALDO ESTOQUE,'
      '    PIE.SALDO_GERENCIAL ESTOQUE_GEREN,'
      '    PIE.SALDO_MINIMO ESTOQUE_MIN,'
      '    PIE.PREC_VENDA,'
      '    PIE.PREC_TAB1,'
      '    PIE.PREC_TAB2,'
      '    PIE.PREC_TAB3,'
      '    PIE.PREC_TAB4,'
      '    PIE.PREC_TAB5,'
      '    PIE.PREC_TAB6,'
      '    PIE.MARGEM_LUCRO_TAB2,'
      '    PIE.MARGEM_LUCRO_TAB3,'
      '    PIE.MARGEM_LUCRO_TAB4,'
      '    PIE.MARGEM_LUCRO_TAB5,'
      '    PIE.MARGEM_LUCRO_TAB6, '
      '    PIE.MARGEM_LUCRO,'
      '    F.NOME FORNECEDOR,'
      '    '#39'N'#39' AS ISNOVO'
      'FROM'
      
        '    PRODUTO_ITEM PI, PRODUTO P, FORNECEDOR F, TAMANHO T, COR C, ' +
        'PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE( '
      '    P.ID = PI.IDPRODUTO'
      'AND'
      '    F.ID = P.IDFORNECEDOR'
      'AND'
      '    T.ID = PI.IDTAMANHO'
      'AND'
      '    C.ID = PI.IDCOR'
      'AND'
      '   PIE.IDITEM = PI.ID'
      'AND'
      '   PIE.IDFILIAL = :IDFILIAL'
      '     ) and (     PI.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PI.*,'
      '    T.NOME TAMANHO,'
      '    T.ALTURA_I,'
      '    T.LARGURA_I,'
      '    T.PROFUNDIDADE_I,'
      '    T.ALTURA_E,'
      '    T.LARGURA_E,'
      '    T.PROFUNDIDADE_E,'
      '    C.NOME COR,'
      '    PIE.SALDO ESTOQUE,'
      '    PIE.SALDO_GERENCIAL ESTOQUE_GEREN,'
      '    PIE.SALDO_MINIMO ESTOQUE_MIN,'
      '    PIE.PREC_VENDA,'
      '    PIE.PREC_TAB1,'
      '    PIE.PREC_TAB2,'
      '    PIE.PREC_TAB3,'
      '    PIE.PREC_TAB4,'
      '    PIE.PREC_TAB5,'
      '    PIE.PREC_TAB6,'
      '    PIE.MARGEM_LUCRO_TAB2,'
      '    PIE.MARGEM_LUCRO_TAB3,'
      '    PIE.MARGEM_LUCRO_TAB4,'
      '    PIE.MARGEM_LUCRO_TAB5,'
      '    PIE.MARGEM_LUCRO_TAB6, '
      '    PIE.MARGEM_LUCRO,'
      '    F.NOME FORNECEDOR,'
      '    '#39'N'#39' AS ISNOVO'
      'FROM'
      
        '    PRODUTO_ITEM PI, PRODUTO P, FORNECEDOR F, TAMANHO T, COR C, ' +
        'PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE'
      '    P.ID = PI.IDPRODUTO'
      'AND'
      '    F.ID = P.IDFORNECEDOR'
      'AND'
      '    T.ID = PI.IDTAMANHO'
      'AND'
      '    C.ID = PI.IDCOR'
      'AND'
      '   PIE.IDITEM = PI.ID'
      'AND'
      '   PIE.IDFILIAL = :IDFILIAL'
      'ORDER BY'
      '   PI.NOME'
      '')
    AutoUpdateOptions.UpdateTableName = 'PRODUTO_ITEM'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_PRODUTO_ITEM'
