    UpdateSQL.Strings = (
      'UPDATE GRUPO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    COMISSAO_DN = :COMISSAO_DN,'
      '    COMISSAO_CH = :COMISSAO_CH,'
      '    COMISSAO_CT = :COMISSAO_CT,'
      '    MARGEM_LUCRO = :MARGEM_LUCRO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    GRUPO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO GRUPO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    ENVIADO_WEB,'
      '    COMISSAO_DN,'
      '    COMISSAO_CH,'
      '    COMISSAO_CT,'
      '    MARGEM_LUCRO'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :ENVIADO_WEB,'
      '    :COMISSAO_DN,'
      '    :COMISSAO_CH,'
      '    :COMISSAO_CT,'
      '    :MARGEM_LUCRO'
      ')')
