      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :IDPRODUTO,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDTAMANHO,'
      '    :IDCOR,'
      '    :PESO,'
      '    :PREC_CUSTO,'
      '    :PREC_CUSTO_MEDIO,'
      '    :COD_FABRICA,'
      '    :PREC_ULTIMO_CUSTO,'
      '    :DT_VALIDADE,'
      '    :DESC_MAX1,'
      '    :DESC_MAX2,'
      '    :DESC_MAX3,'
      '    :DESC_MAX4,'
      '    :COD_FABRICANTE,'
      '    :COMISSAO_DN,'
      '    :COMISSAO_CH,'
      '    :COMISSAO_CT,'
      '    :UNIDADE,'
      '    :ENVIADO_WEB,'
      '    :UTILIZA_BALANCA,'
      '    :OBS,'
      '    :IDENTIFICADOR,'
      '    :INFO_COMPLEMENTAR'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PI.*,'
      '    T.NOME TAMANHO,'
      '    T.ALTURA_I,'
      '    T.LARGURA_I,'
      '    T.PROFUNDIDADE_I,'
      '    T.ALTURA_E,'
      '    T.LARGURA_E,'
      '    T.PROFUNDIDADE_E,'
      '    C.NOME COR,'
      '    PIE.SALDO ESTOQUE,'
      '    PIE.SALDO_GERENCIAL ESTOQUE_GEREN,'
      '    PIE.SALDO_MINIMO ESTOQUE_MIN,'
      '    PIE.PREC_VENDA,'
      '    PIE.PREC_TAB1,'
      '    PIE.PREC_TAB2,'
      '    PIE.PREC_TAB3,'
      '    PIE.PREC_TAB4,'
      '    PIE.PREC_TAB5,'
      '    PIE.PREC_TAB6,'
      '    PIE.MARGEM_LUCRO_TAB2,'
      '    PIE.MARGEM_LUCRO_TAB3,'
      '    PIE.MARGEM_LUCRO_TAB4,'
      '    PIE.MARGEM_LUCRO_TAB5,'
      '    PIE.MARGEM_LUCRO_TAB6, '
      '    PIE.MARGEM_LUCRO,'
      '    '#39'N'#39' AS ISNOVO'
      'FROM'
      '    PRODUTO_ITEM PI, TAMANHO T, COR C, PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE( '
      '    T.ID = PI.IDTAMANHO'
      'AND'
      '    C.ID = PI.IDCOR'
      'AND'
      '   PIE.IDITEM = PI.ID'
      'AND'
      '   PIE.IDFILIAL = :IDFILIAL'
      'AND'
      '   PI.ID = :ID'
      '     ) and (     PI.ID = :OLD_ID'
      '     )'
      '    '
      '')
    SelectSQL.Strings = (
