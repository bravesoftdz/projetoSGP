  object fdsUltimasDatas: TpFIBDataSet
    SelectSQL.Strings = (
      
        'SELECT MAX(DT_ULTIMA_VENDA.DT_INC) ULTIMA_venda, MAX(DT_ULTIMA_C' +
        'OMPRA.DT_INC) ULTIMA_COMPRA'
      'FROM'
      '('
      
        'select NI.DT_INC,NI.IDPROD_ITEM from NFE_ITEM NI, NFE N WHERE NI' +
        '.IDNFE =N.ID AND NI.IDPROD_ITEM =:IDITEM AND N.status = '#39'NF'#39' GRO' +
        'UP BY NI.IDPROD_ITEM,NI.DT_INC'
      'UNION ALL'
      
        'SELECT VI.DT_INC,VI.IDPROD_ITEM from VENDA_ITEM VI,VENDA V  WHER' +
        'E VI.IDVENDA=V.ID AND VI.IDPROD_ITEM=:IDITEM AND STATUS='#39'FC'#39' AND' +
        ' (TIPO = '#39'CF'#39' or (TIPO = '#39'NT'#39')) GROUP BY VI.IDPROD_ITEM,VI.DT_IN' +
        'C'
      ') DT_ULTIMA_VENDA'
      'LEFT JOIN'
      '('
      
        'SELECT C.DT_INC,CI.IDPROD_ITEM from COMPRA_ITEM CI, COMPRA C WHE' +
        'RE C.ID=CI.IDCOMPRA AND CI.IDPROD_ITEM =:IDITEM GROUP BY CI.IDPR' +
        'OD_ITEM,C.DT_INC'
      'UNION ALL'
      
        'SELECT M.DT_INC,MI.IDPROD_ITEM from MOV_INT_ITENS MI, MOVIMENTAC' +
        'AO_INTERNA M  WHERE M.ID = MI.IDMOV AND MI.IDPROD_ITEM=:IDITEM A' +
        'ND STATUS='#39'FC'#39' AND (TIPO = '#39'CF'#39' or (TIPO = '#39'NT'#39')) GROUP BY MI.ID' +
        'PROD_ITEM, M.DT_INC'
      ') DT_ULTIMA_COMPRA'
      'ON'
      ' DT_ULTIMA_VENDA.IDPROD_ITEM =  DT_ULTIMA_COMPRA.IDPROD_ITEM')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 523
    Top = 72
    object fdsUltimasDatasULTIMA_VENDA: TFIBDateField
      FieldName = 'ULTIMA_VENDA'
      DisplayFormat = 'dd/mm/yyyy'
    end
    object fdsUltimasDatasULTIMA_COMPRA: TFIBDateField
      FieldName = 'ULTIMA_COMPRA'
      DisplayFormat = 'dd/mm/yyyy'
    end
  end
  object dsUltimasDatas: TDataSource
    AutoEdit = False
    DataSet = fdsUltimasDatas
    Left = 600
    Top = 64
  end
  object fdsIbpt: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE IBPT'
      'SET '
      '    DT_ALT = :DT_ALT,'
      '    UF = :UF,'
      '    NCM = :NCM,'
      '    DESCRICAO = :DESCRICAO,'
      '    NACIONAL_FEDERAL = :NACIONAL_FEDERAL,'
      '    NACIONAL_IMPORTADOS = :NACIONAL_IMPORTADOS,'
      '    ESTADUAL = :ESTADUAL,'
      '    MUNICIPAL = :MUNICIPAL'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    IBPT'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO IBPT('
      '    ID,'
      '    DT_ALT,'
      '    UF,'
      '    NCM,'
      '    DESCRICAO,'
      '    NACIONAL_FEDERAL,'
      '    NACIONAL_IMPORTADOS,'
      '    ESTADUAL,'
      '    MUNICIPAL'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_ALT,'
      '    :UF,'
      '    :NCM,'
      '    :DESCRICAO,'
      '    :NACIONAL_FEDERAL,'
      '    :NACIONAL_IMPORTADOS,'
      '    :ESTADUAL,'
      '    :MUNICIPAL'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      ' *'
      'FROM'
      '    IBPT '
      ''
      ' WHERE '
      '        IBPT.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      ' I.*'
      'FROM'
      '    IBPT I, CONFIG_NFE C'
      'WHERE'
      ' I.UF = C.WS_UF '
      'AND'
      ' I.NCM = :NCM ')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 691
    Top = 72
