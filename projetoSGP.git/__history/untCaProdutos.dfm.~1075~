  object fdsProduto: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PRODUTO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDGRUPO = :IDGRUPO,'
      '    IDFORNECEDOR = :IDFORNECEDOR,'
      '    DESCRICAO = :DESCRICAO,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    TIPO = :TIPO,'
      '    NCM = :NCM,'
      '    CEST = :CEST,'
      '    CFOP = :CFOP,'
      '    CST = :CST,'
      '    ALIQ_ICMS = :ALIQ_ICMS,'
      '    CSOSN = :CSOSN,'
      '    IBPT_NACIONA_FEDERAL = :IBPT_NACIONA_FEDERAL,'
      '    IBPT_IMPORTADOS_FEDERAL = :IBPT_IMPORTADOS_FEDERAL,'
      '    IBPT_ESTADUAL = :IBPT_ESTADUAL,'
      '    IBPT_MUNICIPAL = :IBPT_MUNICIPAL,'
      '    IDSUBGRUPO = :IDSUBGRUPO,'
      '    INFO_ADICIONAL = :INFO_ADICIONAL,'
      '    FCP = :FCP,'
      '    CST_PIS = :CST_PIS,'
      '    CST_COFINS = :CST_COFINS,'
      '    ALQ_IPI = :ALQ_IPI'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PRODUTO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDGRUPO,'
      '    IDFORNECEDOR,'
      '    DESCRICAO,'
      '    ENVIADO_WEB,'
      '    TIPO,'
      '    NCM,'
      '    CEST,'
      '    CFOP,'
      '    CST,'
      '    ALIQ_ICMS,'
      '    CSOSN,'
      '    IBPT_NACIONA_FEDERAL,'
      '    IBPT_IMPORTADOS_FEDERAL,'
      '    IBPT_ESTADUAL,'
      '    IBPT_MUNICIPAL,'
      '    IDSUBGRUPO,'
      '    INFO_ADICIONAL,'
      '    FCP,'
      '    CST_PIS,'
      '    CST_COFINS,'
      '    ALQ_IPI'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDGRUPO,'
      '    :IDFORNECEDOR,'
      '    :DESCRICAO,'
      '    :ENVIADO_WEB,'
      '    :TIPO,'
      '    :NCM,'
      '    :CEST,'
      '    :CFOP,'
      '    :CST,'
      '    :ALIQ_ICMS,'
      '    :CSOSN,'
      '    :IBPT_NACIONA_FEDERAL,'
      '    :IBPT_IMPORTADOS_FEDERAL,'
      '    :IBPT_ESTADUAL,'
      '    :IBPT_MUNICIPAL,'
      '    :IDSUBGRUPO,'
      '    :INFO_ADICIONAL,'
      '    :FCP,'
      '    :CST_PIS,'
      '    :CST_COFINS,'
      '    :ALQ_IPI'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    P.*,'
      '    F.NOME FORNECEDOR,'
      '    G.NOME GRUPO,'
      '    CF.NOME CFOP_NOME'
      'FROM'
      '    PRODUTO P, FORNECEDOR F, GRUPO G, CFOP CF'
      'WHERE( '
      '    F.ID = P.IDFORNECEDOR'
      'AND'
      '    G.ID = P.IDGRUPO'
      'AND'
      '    CF.CODIGO_CFOP = P.CFOP'
      'AND '
      '    P.ID = :ID'
      '     ) and (     P.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    P.*,'
      '    F.NOME FORNECEDOR,'
      '    G.NOME GRUPO,'
      '    CF.NOME CFOP_NOME'
      'FROM'
      '    PRODUTO P, FORNECEDOR F, GRUPO G, CFOP CF'
      'WHERE'
      '    F.ID = P.IDFORNECEDOR'
      'AND'
      '    G.ID = P.IDGRUPO'
      'AND'
      '    CF.CODIGO_CFOP = P.CFOP'
      'AND '
      '    P.ID = :ID'
      'ORDER BY'
      '    P.NOME')
    AutoUpdateOptions.UpdateTableName = 'PRODUTO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_PRODUTO'
    AutoUpdateOptions.WhenGetGenID = wgOnNewRecord
    AfterScroll = fdsProdutoAfterScroll
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 56
    Top = 8
    oFetchAll = True
    object fdsProdutoID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
    object fdsProdutoDT_INC: TFIBDateField
      DefaultExpression = 'CURRENT_DATE'
      FieldName = 'DT_INC'
