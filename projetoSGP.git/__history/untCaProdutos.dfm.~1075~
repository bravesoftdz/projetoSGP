  object fdsMateriaPrima: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PRODUTO_ITEM_PRODUCAO'
      'SET '
      '    IDITEM_FABRICACAO = :IDITEM_FABRICACAO,'
      '    IDITEM_MATERIA_PRIMA = :IDITEM_MATERIA_PRIMA,'
      '    QTN_MATERIA_PRIMA = :QTN_MATERIA_PRIMA,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PRODUTO_ITEM_PRODUCAO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO_ITEM_PRODUCAO('
      '    ID,'
      '    IDITEM_FABRICACAO,'
      '    IDITEM_MATERIA_PRIMA,'
      '    QTN_MATERIA_PRIMA,'
      '    ENVIADO_WEB'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDITEM_FABRICACAO,'
      '    :IDITEM_MATERIA_PRIMA,'
      '    :QTN_MATERIA_PRIMA,'
      '    :ENVIADO_WEB'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    PIP.*,'
      '    PI.REFERENCIA,'
      '    T.NOME TAMANHO,'
      '    C.NOME COR,'
      '    PIE.SALDO ESTOQUE,'
      '    PIE.SALDO_MINIMO ESTOQUE_MIN,'
      '    PI.NOME,'
      '    PI.COD_FABRICA'
      'FROM'
      
        '     PRODUTO_ITEM_PRODUCAO PIP, PRODUTO_ITEM PI, TAMANHO T, COR ' +
        'C, PRODUTO_ITEM_ESTOQUE PIE'
      '      '
      'WHERE( '
      '    PIP.IDITEM_FABRICACAO = :IDITEM'
      'AND'
      '    PI.ID = PIP.IDITEM_MATERIA_PRIMA'
      'AND'
      '    T.ID = PI.IDTAMANHO'
      'AND'
      '    C.ID = PI.IDCOR'
      'AND'
      '   PIE.IDITEM = PI.ID'
      'AND'
      '   PIE.IDFILIAL = :IDFILIAL'
      '     ) and (     PIP.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    PIP.*,'
      '    PI.REFERENCIA,'
      '    T.NOME TAMANHO,'
      '    C.NOME COR,'
      '    PIE.SALDO ESTOQUE,'
      '    PIE.SALDO_MINIMO ESTOQUE_MIN,'
      '    PI.NOME,'
      '    PI.COD_FABRICA'
      'FROM'
      
        '     PRODUTO_ITEM_PRODUCAO PIP, PRODUTO_ITEM PI, TAMANHO T, COR ' +
        'C, PRODUTO_ITEM_ESTOQUE PIE'
      '      '
      'WHERE'
      '    PIP.IDITEM_FABRICACAO = :IDITEM'
      'AND'
      '    PI.ID = PIP.IDITEM_MATERIA_PRIMA'
      'AND'
      '    T.ID = PI.IDTAMANHO'
      'AND'
      '    C.ID = PI.IDCOR'
      'AND'
      '   PIE.IDITEM = PI.ID'
      'AND'
      '   PIE.IDFILIAL = :IDFILIAL')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 600
    Top = 544
