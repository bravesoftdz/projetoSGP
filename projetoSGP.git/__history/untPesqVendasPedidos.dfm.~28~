  object fdsVendasPedidos: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT '
      '      VP.*,'
      
        '      CASE VP.PREFIXO WHEN '#39'PD'#39' THEN '#39'PEDIDO'#39' WHEN '#39'VE'#39' THEN '#39'VE' +
        'NDA'#39' END AS TIPO '
      'FROM '
      '      V_VENDA_PEDIDO VP'
      'ORDER BY'
      '      VP.PREFIXO, VP.NUMERO DESC')
