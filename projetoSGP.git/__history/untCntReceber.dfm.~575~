  object fdsNotasClientesEmAtraso: TpFIBDataSet
    RefreshSQL.Strings = (
      '')
    SelectSQL.Strings = (
      'select c.nome,c.id,'
      'ci.referencia,'
      'ci.dt_venc,'
      'ci.DT_PAG,'
      'ci.vlr_juros_restante,'
      'ci.valor_juros_pago,'
      'ci.ACRESCIMO,'
      'ci.desconto,'
      'ci.valor AS VALOR_RECEBER,'
      'ci.valor_pago as VALOR_TOT_PAGO,'
      'sum(ci.valor)- sum(ci.valor_pago) as VALOR_DEVEDOR'
      ''
      'from cliente c,cnt_receber cr,cnt_receber_item ci'
      'where'
      ' ci.idreceber=cr.id'
      'and'
      ' c.id = cr.idcliente'
      'and'
      ' cr.posicao= '#39'AB'#39
      'and '
      ' cr.idcliente <> '#39'99999'#39
      'and '
      ' cr.idcliente <> '#39'99998'#39
      'and '
      'ci.dt_venc <= current_date'
      'and '
      'ci.prefixo= '#39'NT'#39
      ''
      
        'group by c.nome,ci.dt_venc,ci.referencia,ci.valor,ci.valor_pago,' +
        'c.id,ci.dt_pag,ci.vlr_juros_restante,ci.valor_juros_pago,ci.ACRE' +
        'SCIMO,ci.desconto'
      'order by c.nome ascending')
    OnCalcFields = fdsNotasClientesEmAtrasoCalcFields
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 626
    Top = 191
