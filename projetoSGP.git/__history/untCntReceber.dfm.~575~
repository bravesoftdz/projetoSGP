  object fdsnotasClientesDevedores: TpFIBDataSet
    UpdateSQL.Strings = (
      ''
      '    ')
    DeleteSQL.Strings = (
      ''
      '    ')
    SelectSQL.Strings = (
      'select c.nome,c.id,Cr.DT_INC,'
      'ci.referencia,'
      'ci.dt_venc,'
      'MAX(CI.DT_PAG) AS DT_ULTIMO_PAG,'
      'ci.valor AS VALOR_RECEBER,'
      'ci.valor_pago as VALOR_TOT_PAGO,'
      'sum(ci.valor)- sum(ci.valor_pago) as VALOR_DEVEDOR'
      ''
      'from cliente c,cnt_receber cr,cnt_receber_item ci'
      'where'
      ' ci.idreceber=cr.id'
      'and'
      ' c.id = cr.idcliente'
      'and'
      ' cr.posicao= '#39'AB'#39
      'and '
      ' cr.idcliente= :IDCLIENTE'
      'and '
      'ci.dt_venc <= current_date'
      ''
      
        'group by c.nome,ci.dt_venc,ci.referencia,ci.valor,ci.valor_pago,' +
        'c.id,Cr.DT_INC'
      'order by c.nome ascending')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 272
    Top = 224
    object fdsnotasClientesDevedoresNOME: TFIBStringField
      FieldName = 'NOME'
      Size = 250
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsnotasClientesDevedoresID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
    object fdsnotasClientesDevedoresDT_INC: TFIBDateField
      FieldName = 'DT_INC'
    end
    object fdsnotasClientesDevedoresREFERENCIA: TFIBStringField
      FieldName = 'REFERENCIA'
      Size = 14
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsnotasClientesDevedoresDT_VENC: TFIBDateField
      FieldName = 'DT_VENC'
    end
