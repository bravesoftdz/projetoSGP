      '    MR.*,'
      '    C.NOME AS CLIENTE ,'
      '    U.NOME AS USUARIO'
      'FROM'
      '    MOVIMENTO_RENEGOCIAR MR, CLIENTE C , USUARIO U'
      'WHERE( '
      '  C.ID= MR.IDCLIENTE'
      'AND '
      '  U.ID= MR.IDUSUARIO'
      '     ) and (     MR.ID = :OLD_ID'
      '     )'
      '    ')
