  object fdsCntReceber: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CNT_RECEBER'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
      '    IDPORTADOR = :IDPORTADOR,'
      '    HISTORICO = :HISTORICO,'
      '    IDFORMA_PAG = :IDFORMA_PAG,'
      '    IDVENDA = :IDVENDA,'
      '    TIPO_VENDA = :TIPO_VENDA,'
      '    TOT_PARCELA = :TOT_PARCELA,'
      '    PARCELAS_PG = :PARCELAS_PG,'
      '    DINHEIRO = :DINHEIRO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CNT_RECEBER'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO CNT_RECEBER('
      '    ID,'
      '    IDFILIAL,'
      '    DT_INC,'
      '    DT_ALT,'
      '    POSICAO,'
      '    PREFIXO,'
      '    REFERENCIA,'
      '    IDCLIENTE,'
      '    ACRESCIMO,'
      '    DESCONTO,'
      '    VALOR,'
      '    VALOR_PAGO,'
      '    IDPORTADOR,'
      '    HISTORICO,'
      '    IDFORMA_PAG,'
      '    IDVENDA,'
      '    TIPO_VENDA,'
      '    TOT_PARCELA,'
      '    PARCELAS_PG,'
      '    DINHEIRO'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :POSICAO,'
      '    :PREFIXO,'
      '    :REFERENCIA,'
      '    :IDCLIENTE,'
      '    :ACRESCIMO,'
      '    :DESCONTO,'
      '    :VALOR,'
      '    :VALOR_PAGO,'
      '    :IDPORTADOR,'
      '    :HISTORICO,'
      '    :IDFORMA_PAG,'
      '    :IDVENDA,'
      '    :TIPO_VENDA,'
      '    :TOT_PARCELA,'
      '    :PARCELAS_PG,'
      '    :DINHEIRO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    R.ID,'
      '    R.IDFILIAL,'
      '    R.DT_INC,'
      '    R.DT_ALT,'
      '    R.POSICAO,'
      '    R.PREFIXO,'
      '    R.REFERENCIA,'
      '    R.IDCLIENTE,'
      '    R.ACRESCIMO,'
      '    R.DESCONTO,'
      '    R.VALOR,'
      '    R.VALOR_PAGO,'
      '    R.IDPORTADOR,'
      '    R.HISTORICO,'
      '    R.IDFORMA_PAG,'
      '    R.IDVENDA,'
      '    R.TIPO_VENDA,'
      '    R.TOT_PARCELA,'
      '    R.PARCELAS_PG,'
      '    R.DINHEIRO,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAG'
      'FROM'
      '    CNT_RECEBER R, CLIENTE C, FORMA_PAGAMENTO FP'
      'WHERE( '
      '    C.ID = R.IDCLIENTE'
      'AND'
      '   FP.ID = R.IDFORMA_PAG'
      '     ) and (     R.ID = :OLD_ID'
      '    and R.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    '
      '')
    SelectSQL.Strings = (
      'SELECT'
      '    R.ID,'
      '    R.IDFILIAL,'
      '    R.DT_INC,'
      '    R.DT_ALT,'
      '    R.POSICAO,'
      '    R.PREFIXO,'
      '    R.REFERENCIA,'
      '    R.IDCLIENTE,'
      '    R.ACRESCIMO,'
      '    R.DESCONTO,'
      '    R.VALOR,'
      '    R.VALOR_PAGO,'
      '    R.IDPORTADOR,'
      '    R.HISTORICO,'
      '    R.IDFORMA_PAG,'
      '    R.IDVENDA,'
      '    R.TIPO_VENDA,'
      '    R.TOT_PARCELA,'
      '    R.PARCELAS_PG,'
      '    R.DINHEIRO,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAG'
      'FROM'
      '    CNT_RECEBER R, CLIENTE C, FORMA_PAGAMENTO FP'
      'WHERE'
      '    C.ID = R.IDCLIENTE'
      'AND'
      '   FP.ID = R.IDFORMA_PAG'
      '')
    AfterClose = fdsCntReceberAfterClose
    BeforeOpen = fdsCntReceberBeforeOpen
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 96
    Top = 8
    oFetchAll = True
    object fdsCntReceberID: TFIBBCDField
