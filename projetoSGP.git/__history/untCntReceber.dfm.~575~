  object qrySomaReceber: TpFIBQuery
    Transaction = DM.transPricipal
    Database = DM.Conexao
    SQL.Strings = (
      
        'SELECT SUM(VALOR) AS TOTAL FROM CNT_RECEBER WHERE POSICAO = :POS' +
        'ICAO')
    Left = 296
  end
  object fdsCntReceberItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CNT_RECEBER_ITEM'
      'SET '
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDRECEBER = :IDRECEBER,'
      '    DT_VENC = :DT_VENC,'
      '    DT_PAG = :DT_PAG,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
      '    HISTORICO = :HISTORICO,'
      '    PARCELA = :PARCELA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    BAIXA_AGRUPADA = :BAIXA_AGRUPADA,'
      '    PAGAMENTO_MOEDA = :PAGAMENTO_MOEDA,'
      '    REC_DESC_ACRE = :REC_DESC_ACRE,'
      '    REC_DINHEIRO = :REC_DINHEIRO,'
      '    ID_REC_ITEM = :ID_REC_ITEM,'
      '    VALOR_JUROS_PAGO = :VALOR_JUROS_PAGO,'
      '    VLR_JUROS_RESTANTE = :VLR_JUROS_RESTANTE,'
      '    VLR_RENEGOCIADO = :VLR_RENEGOCIADO'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    CNT_RECEBER_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO CNT_RECEBER_ITEM('
      '    ID,'
      '    POSICAO,'
      '    PREFIXO,'
      '    REFERENCIA,'
      '    IDRECEBER,'
      '    DT_VENC,'
      '    DT_PAG,'
      '    ACRESCIMO,'
      '    DESCONTO,'
      '    VALOR,'
      '    VALOR_PAGO,'
      '    HISTORICO,'
      '    PARCELA,'
      '    ENVIADO_WEB,'
      '    IDFILIAL,'
      '    BAIXA_AGRUPADA,'
      '    PAGAMENTO_MOEDA,'
      '    REC_DESC_ACRE,'
      '    REC_DINHEIRO,'
      '    ID_REC_ITEM,'
      '    VALOR_JUROS_PAGO,'
      '    VLR_JUROS_RESTANTE,'
      '    VLR_RENEGOCIADO'
      ')'
      'VALUES('
      '    :ID,'
      '    :POSICAO,'
      '    :PREFIXO,'
      '    :REFERENCIA,'
      '    :IDRECEBER,'
      '    :DT_VENC,'
      '    :DT_PAG,'
      '    :ACRESCIMO,'
      '    :DESCONTO,'
      '    :VALOR,'
      '    :VALOR_PAGO,'
      '    :HISTORICO,'
      '    :PARCELA,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL,'
      '    :BAIXA_AGRUPADA,'
      '    :PAGAMENTO_MOEDA,'
      '    :REC_DESC_ACRE,'
      '    :REC_DINHEIRO,'
      '    :ID_REC_ITEM,'
      '    :VALOR_JUROS_PAGO,'
      '    :VLR_JUROS_RESTANTE,'
      '    :VLR_RENEGOCIADO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    RI.*,'
      '    R.IDVENDA,'
      '    R.DT_INC,'
      '    C.NOME CLIENTE,'
      '    C.ID IDCLIENTE,'
      '    C.CREDITO,'
      
        '    CASE RI.prefixo when '#39'CT'#39' THEN '#39'Cart'#227'o de Cr'#233'dito'#39' when '#39'CD'#39 +
        ' then '#39'Cart'#227'o de D'#233'bito'#39' when '#39'CH'#39' THEN '#39'Cheque'#39' when '#39'NT'#39' then ' +
        #39'Nota'#39' when '#39'BL'#39' THEN '#39'Boleto'#39' when '#39'TK'#39' then '#39'Ticket'#39' end DOCUM' +
        'ENTO'
      'FROM'
      '    CNT_RECEBER_ITEM RI, CNT_RECEBER R, CLIENTE C'
      'WHERE( '
      '    R.ID = RI.IDRECEBER'
      'AND    '
      '    C.ID = R.IDCLIENTE'
      '     ) and (     RI.ID = :OLD_ID'
      '    and RI.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    RI.*,'
      '    R.IDVENDA,'
      '    R.DT_INC,'
      '    C.NOME CLIENTE,'
      '    C.ID IDCLIENTE,'
      '    C.CREDITO,'
      
        '    CASE RI.prefixo when '#39'CT'#39' THEN '#39'Cart'#227'o de Cr'#233'dito'#39' when '#39'CD'#39 +
        ' then '#39'Cart'#227'o de D'#233'bito'#39' when '#39'CH'#39' THEN '#39'Cheque'#39' when '#39'NT'#39' then ' +
        #39'Nota'#39' when '#39'BL'#39' THEN '#39'Boleto'#39' when '#39'TK'#39' then '#39'Ticket'#39' end DOCUM' +
        'ENTO'
      'FROM'
      '    CNT_RECEBER_ITEM RI, CNT_RECEBER R, CLIENTE C'
      'WHERE'
      '    R.ID = RI.IDRECEBER'
      'AND    '
      '    C.ID = R.IDCLIENTE'
      'ORDER BY'
      '    RI.POSICAO, RI.DT_VENC')
    AfterScroll = fdsCntReceberItensAfterScroll
    OnCalcFields = fdsCntReceberItensCalcFields
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 126
    Top = 8
    oFetchAll = True
    object fdsCntReceberItensID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
