  object fdsCliente: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '    CL.ID,'
      '    CL.NOME,'
      '    TELEFONE,'
      '    CL.CELULAR,'
      
        '    case CL.TIPO  when '#39'J'#39' then '#39'Pessoa Jur'#237'dica'#39' when '#39'F'#39' then ' +
        #39'Pessoa F'#237'sica'#39' end AS TIPO,'
      '    CI.NOME CIDADE'
      'FROM'
      '    CLIENTE CL, CIDADE_IBGE CI'
      'WHERE'
      '    CI.ID = CL.IDCIDADE')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
