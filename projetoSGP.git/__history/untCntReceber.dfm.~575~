  object fdsClienteAtraso: TpFIBDataSet
    SelectSQL.Strings = (
      'select '
      'c.nome,'
      'sum(ci.valor)as VALOR_RECEBER_TOTAL ,'
      'sum(ci.valor_pago)as VALOR_TOT_PAGO_TOTAL,'
      'sum(ci.valor)- sum(ci.valor_pago) as VALOR_DEVEDOR_TOTAL'
      ''
      'from '
      'cliente c,cnt_receber cr,cnt_receber_item ci'
      ' where'
      ' ci.idreceber = cr.id'
      ' and'
      ' c.id = cr.idcliente'
      ' and'
      ' cr.posicao= '#39'AB'#39
      ' and '
      ' cr.idcliente <> '#39'99999'#39
      ' and '
      ' cr.idcliente <> '#39'99998'#39
      ' and '
      ' ci.dt_venc <= current_date'
      ' and'
      ' ci.prefixo= '#39'NT'#39
      ''
      'group by c.nome'
      'order by valor_devedor_TOTAL descending')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 746
    Top = 231
    oFetchAll = True
    object fdsClienteAtrasoNOME: TFIBStringField
      FieldName = 'NOME'
