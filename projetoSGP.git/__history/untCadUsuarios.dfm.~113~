  object fdsUsuarios: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE USUARIO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    NOME = :NOME,'
      '    LOGIN = :LOGIN,'
      '    SENHA = :SENHA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    IDFILIAL = :IDFILIAL'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    USUARIO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO USUARIO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    NOME,'
      '    LOGIN,'
      '    SENHA,'
      '    ENVIADO_WEB,'
      '    IDFILIAL'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :NOME,'
      '    :LOGIN,'
      '    :SENHA,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    USUARIO '
      ''
      ' WHERE '
      '        USUARIO.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    USUARIO '
      'WHERE '
      '   ID <> 1')
    AfterScroll = fdsUsuariosAfterScroll
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 464
    Top = 16
    object fdsUsuariosID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
