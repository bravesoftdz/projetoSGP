  object fdsComissaoVend: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE COMISSAO_VENDEDOR'
      'SET '
      '    IDFILIAL = :IDFILIAL,'
      '    IDVENDEDOR = :IDVENDEDOR,'
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    DT_INICIAL = :DT_INICIAL,'
