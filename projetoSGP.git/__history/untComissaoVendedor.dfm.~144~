  object fdsGrupo: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE GRUPO'
      'SET '
      '    COMISSAO = :COMISSAO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    GRUPO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO GRUPO('
      '    ID,'
      '    COMISSAO'
      ')'
      'VALUES('
      '    :ID,'
      '    :COMISSAO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    COMISSAO'
      'FROM'
      '    GRUPO '
      ''
      ' WHERE '
      '        GRUPO.ID = :OLD_ID'
      '    ')
