  object fdsVendedor: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDEDOR'
      'SET '
      '    NOME = :NOME'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDEDOR'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDEDOR('
      '    ID,'
      '    NOME'
      ')'
      'VALUES('
      '    :ID,'
      '    :NOME'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME'
      'FROM'
      '    VENDEDOR'
      ''
      ' WHERE '
      '        VENDEDOR.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME'
      'FROM'
      '    VENDEDOR')
    AfterOpen = fdsVendedorAfterOpen
    AfterScroll = fdsVendedorAfterScroll
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
