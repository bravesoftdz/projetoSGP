      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    COMISSAO_VENDEDOR'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO COMISSAO_VENDEDOR('
      '    ID,'
      '    IDFILIAL,'
      '    IDVENDEDOR,'
      '    DT_INC,'
      '    DT_ALT,'
      '    DT_INICIAL,'
      '    DT_FINAL,'
      '    POSICAO,'
