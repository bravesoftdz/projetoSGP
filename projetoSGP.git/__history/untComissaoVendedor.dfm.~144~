  object fdsItensVendidos: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA_ITEM'
      'SET '
      '    VLR_UNITARIO = :VLR_UNITARIO'
      'WHERE'
      '    IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA_ITEM'
      'WHERE'
      '        IDVENDA = :OLD_IDVENDA'
      '    and ITEM = :OLD_ITEM'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA_ITEM('
      '    IDVENDA,'
      '    ITEM,'
      '    VLR_UNITARIO'
      ')'
      'VALUES('
      '    :IDVENDA,'
      '    :ITEM,'
      '    :VLR_UNITARIO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      
        '    I.COD_FABRICA, I.NOME, VI.IDVENDA, VI.ITEM, (VI.QTN - VI.QTN' +
        '_RETORNO) AS QTN, VI.VLR_UNITARIO, V.IDFORMA_PAG,'
      
        '     (VI.VLR_UNITARIO * (VI.QTN - VI.QTN_RETORNO)) AS VLR_TOTAL,' +
        ' V.DT_INC, I.COMISSAO_DN, I.COMISSAO_CH, I.COMISSAO_CT '
      'FROM'
      '    VENDA_ITEM VI, produto_item I, VENDA V'
      'WHERE(      '
      
        '     I.id = VI.idprod_item AND V.ID = VI.idvenda AND VI.IDFILIAL' +
        ' = V.IDFILIAL'
      'AND '
      '     V.STATUS <> '#39'AB'#39
      'AND '
      '     V.STATUS <> '#39'CA'#39
      'AND '
      '     V.STATUS <> '#39'DV'#39
      '     ) and (     VI.IDVENDA = :OLD_IDVENDA'
      '    and VI.ITEM = :OLD_ITEM'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      
        '    I.COD_FABRICA, VVPI.NOMEPRODUTO, VVPI.IDVENDA, VVPI.ITEM, (V' +
        'VPI.QTN - VVPI.QTN_RETORNO) AS QTN, (VVPI.VLR_UNITARIO - VVPI.VL' +
        'R_DESC_ACRE) AS VLR_UNITARIO, VVP.IDFORMA_PAG,'
      
        '     ((VVPI.VLR_UNITARIO - VVPI.VLR_DESC_ACRE) * (VVPI.QTN - VVP' +
        'I.QTN_RETORNO)) AS VLR_TOTAL, VVP.DT_INC, VVP.PREFIXO, I.COMISSA' +
        'O_DN, I.COMISSAO_CH, I.COMISSAO_CT'
      'FROM'
      '    V_VENDA_PEDIDO_ITEM VVPI ,V_VENDA_PEDIDO VVP, PRODUTO_ITEM I'
      'WHERE '
      '     VVP.STATUS <> '#39'AB'#39
      'AND '
      '     VVP.STATUS <> '#39'CA'#39
      'AND '
      '     VVP.STATUS <> '#39'DV'#39
      'AND'
      '     I.ID = VVPI.IDPROD_ITEM '
      'AND'
      '     VVPI.PREFIXO = VVP.PREFIXO'
      'AND'
      '     VVPI.IDVENDA = VVP.NUMERO'
      'AND'
      '     VVPI.IDFILIAL = VVP.IDFILIAL')
    OnCalcFields = fdsItensVendidosCalcFields
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 504
    object fdsItensVendidosCOD_FABRICA: TFIBStringField
      FieldName = 'COD_FABRICA'
      Size = 13
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsItensVendidosIDVENDA: TFIBBCDField
      FieldName = 'IDVENDA'
      Size = 0
    end
    object fdsItensVendidosITEM: TFIBSmallIntField
      FieldName = 'ITEM'
    end
    object fdsItensVendidosQTN: TFIBBCDField
      FieldName = 'QTN'
      Size = 8
    end
    object fdsItensVendidosVLR_UNITARIO: TFIBBCDField
      DefaultExpression = '0,00'
      FieldName = 'VLR_UNITARIO'
      DisplayFormat = 'R$ #,##0.00'
      Size = 8
    end
    object fdsItensVendidosIDFORMA_PAG: TFIBBCDField
      FieldName = 'IDFORMA_PAG'
      Size = 0
    end
    object fdsItensVendidosVLR_TOTAL: TFIBFloatField
      DefaultExpression = '0,00'
      FieldName = 'VLR_TOTAL'
      DisplayFormat = 'R$ #,##0.00'
    end
    object fdsItensVendidosDT_INC: TFIBDateField
      FieldName = 'DT_INC'
    end
    object fdsItensVendidosCOMISSAO_DN: TFIBBCDField
      FieldName = 'COMISSAO_DN'
