    RefreshSQL.Strings = (
      'SELECT'
      '    CI.*,'
      '    I.NOME,'
      '    I.COD_FABRICA,'
      '    P.TIPO'
      'FROM'
      '    COMPRA_ITEM CI, PRODUTO_ITEM I, PRODUTO P'
      'WHERE(  '
      '    I.ID = CI.IDPROD_ITEM'
      'AND'
      '    P.ID = I.IDPRODUTO'
      '     ) and (     CI.IDCOMPRA = :OLD_IDCOMPRA'
      '    and CI.IDFILIAL = :OLD_IDFILIAL'
      '    and CI.ITEM = :OLD_ITEM'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    CI.*,'
      '    I.NOME,'
      '    I.COD_FABRICA,'
      '    P.TIPO'
      'FROM'
      '    COMPRA_ITEM CI, PRODUTO_ITEM I, PRODUTO P'
      'WHERE '
      '    I.ID = CI.IDPROD_ITEM'
      'AND'
      '    P.ID = I.IDPRODUTO')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 313
    Top = 10
