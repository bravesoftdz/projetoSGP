      'WHERE'
      '    IDCOMPRA = :OLD_IDCOMPRA'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    and ITEM = :OLD_ITEM'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    COMPRA_ITEM'
      'WHERE'
      '        IDCOMPRA = :OLD_IDCOMPRA'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    and ITEM = :OLD_ITEM'
      '    ')
