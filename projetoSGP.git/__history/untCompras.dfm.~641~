      '    FL.NOME FILIAL'
      'FROM'
      '    COMPRA C, FORNECEDOR F, FILIAL FL'
      'WHERE'
      '    F.ID =  C.IDFORNECEDOR'
      'AND'
      '   FL.ID = C.IDFILIAL')
