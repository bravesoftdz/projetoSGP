    RefreshSQL.Strings = (
      'SELECT'
      '    C.*,'
      
        '    case C.status  when '#39'A'#39' then '#39'ATIVO'#39' when '#39'C'#39' then '#39'CANCELAD' +
        'O'#39' end AS ST,'
      '    F.NOME FORNECEDOR,'
      '    FL.NOME FILIAL'
      'FROM'
      '    COMPRA C, FORNECEDOR F, FILIAL FL'
      'WHERE( '
      '    F.ID =  C.IDFORNECEDOR'
      'AND'
      '   FL.ID = C.IDFILIAL'
      '     ) and (     C.ID = :OLD_ID'
      '    and C.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
