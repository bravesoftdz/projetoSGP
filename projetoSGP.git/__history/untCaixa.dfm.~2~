  object fdsVenda: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDA'
      'SET '
      '    IDCAIXA = :IDCAIXA,'
      '    DT_INC = :DT_INC,'
      '    STATUS = :STATUS,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    IDVENDEDOR = :IDVENDEDOR,'
      '    IDFORMA_PAG = :IDFORMA_PAG,'
      '    VLR_TOTITEM = :VLR_TOTITEM,'
      '    VLR_DESCONTO = :VLR_DESCONTO,'
      '    VLR_ENTRADA = :VLR_ENTRADA,'
      '    VLR_FRETE = :VLR_FRETE,'
      '    VLR_TOTAL = :VLR_TOTAL,'
      '    PESO = :PESO,'
      '    PREFIXO = :PREFIXO,'
      '    DT_FECHAMENTO = :DT_FECHAMENTO,'
      '    SUBTRIBUTARIA = :SUBTRIBUTARIA,'
      '    ENVIADO_WEB = :ENVIADO_WEB,'
      '    ENTREGAR = :ENTREGAR,'
      '    OBS = :OBS,'
      '    TOT_PARCELA = :TOT_PARCELA'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDA('
      '    ID,'
      '    IDFILIAL,'
      '    IDCAIXA,'
      '    DT_INC,'
      '    STATUS,'
      '    IDCLIENTE,'
      '    IDVENDEDOR,'
      '    IDFORMA_PAG,'
      '    VLR_TOTITEM,'
      '    VLR_DESCONTO,'
      '    VLR_ENTRADA,'
      '    VLR_FRETE,'
      '    VLR_TOTAL,'
      '    PESO,'
      '    PREFIXO,'
      '    DT_FECHAMENTO,'
      '    SUBTRIBUTARIA,'
      '    ENVIADO_WEB,'
      '    ENTREGAR,'
      '    OBS,'
      '    TOT_PARCELA'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :IDCAIXA,'
      '    :DT_INC,'
      '    :STATUS,'
      '    :IDCLIENTE,'
      '    :IDVENDEDOR,'
      '    :IDFORMA_PAG,'
      '    :VLR_TOTITEM,'
      '    :VLR_DESCONTO,'
      '    :VLR_ENTRADA,'
      '    :VLR_FRETE,'
      '    :VLR_TOTAL,'
      '    :PESO,'
      '    :PREFIXO,'
      '    :DT_FECHAMENTO,'
      '    :SUBTRIBUTARIA,'
      '    :ENVIADO_WEB,'
      '    :ENTREGAR,'
      '    :OBS,'
      '    :TOT_PARCELA'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    V.*,'
      '    F.NOME FILIAL,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAG,'
      '    VE.NOME VENDEDOR'
      'FROM'
      
        '    VENDA V, FILIAL F, CLIENTE C, FORMA_PAGAMENTO FP, VENDEDOR V' +
        'E'
      'WHERE( '
      '    F.ID = V.IDFILIAL'
      'AND'
      '    C.ID = V.IDCLIENTE'
      'AND '
      '    FP.ID = V.IDFORMA_PAG'
      'AND'
      '    VE.ID = V.IDVENDEDOR'
      '     ) and (     V.ID = :OLD_ID'
      '    and V.IDFILIAL = :OLD_IDFILIAL'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    V.*,'
      '    F.NOME FILIAL,'
      '    C.NOME CLIENTE,'
      '    FP.NOME FORMA_PAG,'
      '    VE.NOME VENDEDOR'
      'FROM'
      
        '    VENDA V, FILIAL F, CLIENTE C, FORMA_PAGAMENTO FP, VENDEDOR V' +
        'E'
      'WHERE'
      '    F.ID = V.IDFILIAL'
      'AND'
      '    C.ID = V.IDCLIENTE'
      'AND '
      '    FP.ID = V.IDFORMA_PAG'
      'AND'
      '    VE.ID = V.IDVENDEDOR')
    AllowedUpdateKinds = []
    Transaction = DM.transPricipal
    Database = DM.Conexao
    SQLScreenCursor = crSQLWait
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    oFetchAll = True
