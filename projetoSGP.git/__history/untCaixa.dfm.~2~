  object fdsFilial: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE FILIAL'
      'SET '
      '    STATUS = :STATUS,'
      '    NOME = :NOME'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    FILIAL'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO FILIAL('
      '    ID,'
      '    STATUS,'
      '    NOME'
      ')'
      'VALUES('
      '    :ID,'
      '    :STATUS,'
      '    :NOME'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    STATUS,'
      '    NOME'
      'FROM'
      '    FILIAL '
      ''
      ' WHERE '
      '        FILIAL.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    STATUS,'
      '    NOME'
      'FROM'
      '    FILIAL ')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    SQLScreenCursor = crSQLWait
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    oFetchAll = True
    object fdsFilialID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
      RoundByScale = True
    end
    object fdsFilialSTATUS: TFIBStringField
      FieldName = 'STATUS'
      Size = 1
      EmptyStrToNull = True
    end
    object fdsFilialNOME: TFIBStringField
      FieldName = 'NOME'
      Size = 250
      EmptyStrToNull = True
    end
  end
  object dsFilial: TDataSource
    AutoEdit = False
    DataSet = fdsFilial
  end
  object fdsCliente: TpFIBDataSet
    SelectSQL.Strings = (
      'SELECT'
      '    CL.ID,'
      '    CL.NOME,'
      '    TELEFONE,'
      
        '    case CL.TIPO  when '#39'J'#39' then '#39'Pessoa Jur'#237'dica'#39' when '#39'F'#39' then ' +
        #39'Pessoa F'#237'sica'#39' end AS TIPO,'
      '    CI.NOME CIDADE'
      'FROM'
      '    CLIENTE CL, CIDADE_IBGE CI'
      'WHERE'
      '    CI.ID = CL.IDCIDADE')
    Transaction = DM.transPricipal
    Database = DM.Conexao
    SQLScreenCursor = crSQLWait
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    oFetchAll = True
    object fdsClienteID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
      RoundByScale = True
    end
    object fdsClienteNOME: TFIBStringField
      FieldName = 'NOME'
      Size = 250
      EmptyStrToNull = True
    end
    object fdsClienteTELEFONE: TFIBStringField
      FieldName = 'TELEFONE'
      Size = 13
      EmptyStrToNull = True
    end
    object fdsClienteTIPO: TFIBStringField
