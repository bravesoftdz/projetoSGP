  object fdsFuncionarios: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE FUNCIONARIO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    DT_NASC = :DT_NASC,'
      '    TELEFONE = :TELEFONE,'
      '    ENDERECO = :ENDERECO,'
      '    BAIRRO = :BAIRRO,'
      '    IDCIDADE = :IDCIDADE,'
      '    CEP = :CEP,'
      '    RG = :RG,'
      '    CPF = :CPF,'
      '    SEXO = :SEXO,'
      '    CELULAR = :CELULAR,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    FUNCIONARIO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO FUNCIONARIO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    DT_NASC,'
      '    TELEFONE,'
      '    ENDERECO,'
      '    BAIRRO,'
      '    IDCIDADE,'
      '    CEP,'
      '    RG,'
      '    CPF,'
      '    SEXO,'
      '    CELULAR,'
      '    ENVIADO_WEB'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :DT_NASC,'
      '    :TELEFONE,'
      '    :ENDERECO,'
      '    :BAIRRO,'
      '    :IDCIDADE,'
      '    :CEP,'
      '    :RG,'
      '    :CPF,'
      '    :SEXO,'
      '    :CELULAR,'
      '    :ENVIADO_WEB'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    F.*,'
      '    CI.NOME'
      'FROM'
      '    FUNCIONARIO F, CIDADE_IBGE CI'
      'WHERE( '
      '    CI.ID = F.IDCIDADE'
      '     ) and (     F.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    F.*,'
      '    CI.NOME CIDADE,'
      '    CI.UF UF'
      'FROM'
      '    FUNCIONARIO F, CIDADE_IBGE CI'
      'WHERE'
      '    CI.ID = F.IDCIDADE')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 384
    Top = 8
    oFetchAll = True
