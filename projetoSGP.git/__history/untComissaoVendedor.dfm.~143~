      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    COMISSAO_VENDEDOR'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO COMISSAO_VENDEDOR('
      '    ID,'
      '    IDFILIAL,'
      '    IDVENDEDOR,'
      '    DT_INC,'
      '    DT_ALT,'
      '    DT_INICIAL,'
      '    DT_FINAL,'
      '    POSICAO,'
      '    VLR_TOT_VENDAS,'
      '    VLR_COMISSAO'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDFILIAL,'
      '    :IDVENDEDOR,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :DT_INICIAL,'
      '    :DT_FINAL,'
      '    :POSICAO,'
      '    :VLR_TOT_VENDAS,'
      '    :VLR_COMISSAO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    CV.*,'
      '    V.NOME'
      'FROM'
      '    COMISSAO_VENDEDOR CV, VENDEDOR V '
      'WHERE( '
      '    V.ID = CV.IDVENDEDOR'
      '     ) and (     CV.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    CV.*,'
      '    V.NOME'
      'FROM'
      '    COMISSAO_VENDEDOR CV, VENDEDOR V '
      'WHERE'
      '    V.ID = CV.IDVENDEDOR'
      'ORDER BY'
      '    CV.POSICAO'
      '')
    AfterScroll = fdsComissaoVendAfterScroll
    Transaction = DM.transPricipal
    Database = DM.Conexao
    Left = 397
