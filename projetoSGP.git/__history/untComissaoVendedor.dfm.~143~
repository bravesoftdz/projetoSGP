      '    IDVENDEDOR = :IDVENDEDOR,'
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    DT_INICIAL = :DT_INICIAL,'
      '    DT_FINAL = :DT_FINAL,'
