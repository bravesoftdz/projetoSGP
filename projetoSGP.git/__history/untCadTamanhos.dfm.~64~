  object fdsTamanho_Grupo: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE TAMANHO_GRUPO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS'
      'WHERE'
      '    IDTAMANHO = :OLD_IDTAMANHO'
      '    and IDGRUPO = :OLD_IDGRUPO'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    TAMANHO_GRUPO'
      'WHERE'
      '        IDTAMANHO = :OLD_IDTAMANHO'
      '    and IDGRUPO = :OLD_IDGRUPO'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO TAMANHO_GRUPO('
      '    IDTAMANHO,'
      '    IDGRUPO,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS'
      ')'
      'VALUES('
      '    :IDTAMANHO,'
      '    :IDGRUPO,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    TM.IDTAMANHO,'
      '    TM.IDGRUPO,'
      '    TM.DT_INC,'
      '    TM.DT_ALT,'
      '    TM.STATUS,'
      
        '    case TM.status  when '#39'A'#39' then '#39'Ativo'#39' when '#39'I'#39' then '#39'inativo' +
        #39' end AS ST,'
      '    G.NOME AS GRUPO'
      'FROM'
      '    TAMANHO_GRUPO TM, GRUPO G'
      'WHERE( '
      '    IDTAMANHO = :IDTAMANHO '
      'AND'
      '    G.ID = IDGRUPO'
      '     ) and (     TM.IDTAMANHO = :OLD_IDTAMANHO'
      '    and TM.IDGRUPO = :OLD_IDGRUPO'
      '     )'
      '     ')
    SelectSQL.Strings = (
      'SELECT'
      '    TM.IDTAMANHO,'
      '    TM.IDGRUPO,'
      '    TM.DT_INC,'
      '    TM.DT_ALT,'
      '    TM.STATUS,'
      
        '    case TM.status  when '#39'A'#39' then '#39'Ativo'#39' when '#39'I'#39' then '#39'inativo' +
        #39' end AS ST,'
      '    G.NOME AS GRUPO'
      'FROM'
      '    TAMANHO_GRUPO TM, GRUPO G'
      'WHERE'
      '    IDTAMANHO = :IDTAMANHO '
      'AND'
      '    G.ID = IDGRUPO ')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 232
    Top = 8
    oFetchAll = True
    object fdsTamanho_GrupoIDTAMANHO: TFIBBCDField
      FieldName = 'IDTAMANHO'
      Size = 0
    end
    object fdsTamanho_GrupoIDGRUPO: TFIBBCDField
      FieldName = 'IDGRUPO'
      Size = 0
    end
    object fdsTamanho_GrupoDT_INC: TFIBDateField
      FieldName = 'DT_INC'
      DisplayFormat = 'dd/mm/yyyy'
    end
    object fdsTamanho_GrupoDT_ALT: TFIBDateField
      FieldName = 'DT_ALT'
      DisplayFormat = 'dd/mm/yyyy'
    end
    object fdsTamanho_GrupoSTATUS: TFIBStringField
      FieldName = 'STATUS'
      Size = 1
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsTamanho_GrupoST: TFIBStringField
      FieldName = 'ST'
      Size = 7
      Transliterate = False
      EmptyStrToNull = True
    end
    object fdsTamanho_GrupoGRUPO: TFIBStringField
      FieldName = 'GRUPO'
      Size = 250
      Transliterate = False
      EmptyStrToNull = True
    end
  end
