  object fdsTamanho: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE TAMANHO'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    ALTURA_I = :ALTURA_I,'
      '    LARGURA_I = :LARGURA_I,'
      '    PROFUNDIDADE_I = :PROFUNDIDADE_I,'
      '    ALTURA_E = :ALTURA_E,'
      '    LARGURA_E = :LARGURA_E,'
      '    PROFUNDIDADE_E = :PROFUNDIDADE_E'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    TAMANHO'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO TAMANHO('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    REFERENCIA,'
      '    ALTURA_I,'
      '    LARGURA_I,'
      '    PROFUNDIDADE_I,'
      '    ALTURA_E,'
      '    LARGURA_E,'
      '    PROFUNDIDADE_E'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :ALTURA_I,'
      '    :LARGURA_I,'
      '    :PROFUNDIDADE_I,'
      '    :ALTURA_E,'
      '    :LARGURA_E,'
      '    :PROFUNDIDADE_E'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    T.*'
      'FROM'
      '    TAMANHO T'
      ''
      ' WHERE '
      '        T.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    T.*'
      'FROM'
      '    TAMANHO T')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
