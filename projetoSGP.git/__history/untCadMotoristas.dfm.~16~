  object fdsMotorista: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE MOTORISTA'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    STATUS = :STATUS,'
      '    NOME = :NOME,'
      '    NCNH = :NCNH,'
      '    TIPOCNH = :TIPOCNH,'
      '    ENVIADO_WEB = :ENVIADO_WEB'
      'WHERE'
      '    ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    MOTORISTA'
      'WHERE'
      '        ID = :OLD_ID'
      '    and IDFILIAL = :OLD_IDFILIAL'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO MOTORISTA('
      '    ID,'
      '    DT_INC,'
      '    DT_ALT,'
      '    STATUS,'
      '    NOME,'
      '    NCNH,'
      '    TIPOCNH,'
      '    ENVIADO_WEB,'
      '    IDFILIAL'
      ')'
      'VALUES('
      '    :ID,'
      '    :DT_INC,'
      '    :DT_ALT,'
      '    :STATUS,'
      '    :NOME,'
      '    :NCNH,'
      '    :TIPOCNH,'
      '    :ENVIADO_WEB,'
      '    :IDFILIAL'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    MOTORISTA '
      ''
      ' WHERE '
      '        MOTORISTA.ID = :OLD_ID'
      '    and MOTORISTA.IDFILIAL = :OLD_IDFILIAL'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    *'
      'FROM'
      '    MOTORISTA ')
    AutoUpdateOptions.UpdateTableName = 'ALUNO'
    AutoUpdateOptions.KeyFields = 'ID'
    AutoUpdateOptions.GeneratorName = 'GEN_ID_ALUNO'
    Transaction = DM.transPricipal
    Database = DM.Conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 320
    Top = 8
    oFetchAll = True
