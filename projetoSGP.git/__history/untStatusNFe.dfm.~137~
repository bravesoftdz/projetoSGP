  object fdsContReceber: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE CNT_RECEBER'
      'SET '
      '    DT_INC = :DT_INC,'
      '    DT_ALT = :DT_ALT,'
      '    POSICAO = :POSICAO,'
      '    PREFIXO = :PREFIXO,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDCLIENTE = :IDCLIENTE,'
      '    ACRESCIMO = :ACRESCIMO,'
      '    DESCONTO = :DESCONTO,'
      '    VALOR = :VALOR,'
      '    VALOR_PAGO = :VALOR_PAGO,'
