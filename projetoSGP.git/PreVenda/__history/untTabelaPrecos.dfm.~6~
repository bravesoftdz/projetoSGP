  object fdsProdutoItens: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE PRODUTO_ITEM'
      'SET '
      '    IDPRODUTO = :IDPRODUTO,'
      '    NOME = :NOME,'
      '    REFERENCIA = :REFERENCIA,'
      '    IDTAMANHO = :IDTAMANHO,'
      '    IDCOR = :IDCOR,'
      '    PESO = :PESO'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    PRODUTO_ITEM'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO PRODUTO_ITEM('
      '    ID,'
      '    IDPRODUTO,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDTAMANHO,'
      '    IDCOR,'
      '    PESO'
      ')'
      'VALUES('
      '    :ID,'
      '    :IDPRODUTO,'
      '    :NOME,'
      '    :REFERENCIA,'
      '    :IDTAMANHO,'
      '    :IDCOR,'
      '    :PESO'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    IDPRODUTO,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDTAMANHO,'
      '    IDCOR,'
      '    PESO,'
      '    PIE.PREC_VENDA,'
      '    PIE.PREC_TAB1,'
      '    PIE.PREC_TAB2,'
      '    PIE.PREC_TAB3,'
      '    PIE.PREC_TAB4,'
      '    PIE.PREC_TAB5,'
      '    PIE.PREC_TAB6,'
      '    PIE.SALDO'
      '    '
      'FROM'
      '    PRODUTO_ITEM PI, PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE( '
      '    PIE.IDITEM = PI.ID'
      'AND'
      '    PIE.IDFILIAL = :IDFILIAL'
      '     ) and (     PI.ID = :OLD_ID'
      '     )'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    IDPRODUTO,'
      '    NOME,'
      '    REFERENCIA,'
      '    IDTAMANHO,'
      '    IDCOR,'
      '    PESO,'
      '    PIE.PREC_VENDA,'
      '    PIE.PREC_TAB1,'
      '    PIE.PREC_TAB2,'
      '    PIE.PREC_TAB3,'
      '    PIE.PREC_TAB4,'
      '    PIE.PREC_TAB5,'
      '    PIE.PREC_TAB6,'
      '    PIE.SALDO'
      '    '
      'FROM'
      '    PRODUTO_ITEM PI, PRODUTO_ITEM_ESTOQUE PIE'
      'WHERE'
      '    PIE.IDITEM = PI.ID'
      'AND'
      '    PIE.IDFILIAL = :IDFILIAL'
      'ORDER BY NOME'
      ''
      '')
    BeforeOpen = fdsProdutoItensBeforeOpen
    Transaction = DM.trans
    Database = DM.conexao
    DefaultFormats.DateTimeDisplayFormat = 'dd/mm/yyyy hh:mm:ss'
    DefaultFormats.DisplayFormatDate = 'dd/mm/yyyy'
    DefaultFormats.DisplayFormatTime = 'hh:mm:ss'
    Left = 256
    Top = 8
    oFetchAll = True
    object fdsProdutoItensID: TFIBBCDField
      FieldName = 'ID'
