  object fdsVendedor: TpFIBDataSet
    UpdateSQL.Strings = (
      'UPDATE VENDEDOR'
      'SET '
      '    NOME = :NOME'
      'WHERE'
      '    ID = :OLD_ID'
      '    ')
    DeleteSQL.Strings = (
      'DELETE FROM'
      '    VENDEDOR'
      'WHERE'
      '        ID = :OLD_ID'
      '    ')
    InsertSQL.Strings = (
      'INSERT INTO VENDEDOR('
      '    ID,'
      '    NOME'
      ')'
      'VALUES('
      '    :ID,'
      '    :NOME'
      ')')
    RefreshSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME    '
      'FROM'
      '    VENDEDOR '
      ''
      ' WHERE '
      '        VENDEDOR.ID = :OLD_ID'
      '    ')
    SelectSQL.Strings = (
      'SELECT'
      '    ID,'
      '    NOME    '
      'FROM'
      '    VENDEDOR')
    Transaction = DM.trans
    Database = DM.conexao
    Left = 191
    Top = 8
    object fdsVendedorID: TFIBBCDField
      FieldName = 'ID'
      Size = 0
    end
    object fdsVendedorNOME: TFIBStringField
      FieldName = 'NOME'
      Size = 250
